(all,i,ENDW_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTalleffir(i,r,t)
        = [0.01 * EVSCALFACT(r,t)]
        * [PTAX(i,r,t) * [qo(i,r,t) - pop(r,t)]
            + sum(j,PROD_COMM, ETAX(i,j,r,t) * [qfe(i,j,r,t) - pop(r,t)])];
Equation CONT_EV_alleffir_T
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTalleffir(i,r,t)
        = [0.01 * EVSCALFACT(r,t)]
        * [PTAX(i,r,t) * [qo(i,r,t) - pop(r,t)]
            + sum(j,PROD_COMM, IFTAX(i,j,r,t) * [qfm(i,j,r,t) - pop(r,t)])
            + sum(j,PROD_COMM, DFTAX(i,j,r,t) * [qfd(i,j,r,t) - pop(r,t)])
            + IPTAX(i,r,t) * [qpm(i,r,t) - pop(r,t)]
            + DPTAX(i,r,t) * [qpd(i,r,t) - pop(r,t)]
            + IGTAX(i,r,t) * [qgm(i,r,t) - pop(r,t)]
            + DGTAX(i,r,t) * [qgd(i,r,t) - pop(r,t)]
            + sum(s,REG, XTAXD(i,r,s,t) * [qxs(i,r,s,t) - pop(r,t)])
            + sum(s,REG, MTAX(i,s,r,t) * [qxs(i,s,r,t) - pop(r,t)])];

