!<
    ------------------
    A. Summary Indices
    ------------------

    The following equations calculate many useful summary statistics.  They do
    not generally affect the equilibrium structure of the model, although they
    do include the equation for the usual numeraire variable, "pfactwld". Some
    are documented in Hertel and Tsigas, many are new.

    A-0. Appendix-Specific Variables and Coefficients
    A-1. Factor Price Indices
    A-2. Regional Terms of Trade
    A-3. GDP Indices (Value, Price and Quantity)
    A-4. Aggregate Trade Indices (Value, Price and Quantity)
    A-5. Trade Balance Indices
>!


!<
    A-0. Appendix-Specific Variables and Coefficients
    -------------------------------------------------

    only used in this Summary Indices appendix
>!

Variable (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    vxwfob(i,s,t) # value of merchandise regional exports, by commodity, FOB #;
Variable (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    viwcif(i,s,t) # value of merchandise regional imports, by commodity, CIF #;
Variable (all,r,REG)(all,t,ALLTIME)
    vxwreg(r,t) # value of merchandise exports, by region #;
Variable (all,r,REG)(all,t,ALLTIME)
    viwreg(r,t) # value of merchandise imports, by region, at world prices #;

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(i,r,t) # value of exports by comm. i and region r at FOB prices #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(m,r,t) = sum(s,REG, VXWD(m,r,s,t)) + VST(m,r,t);
Formula (all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(i,r,t) = sum(s,REG, VXWD(i,r,s,t));

Coefficient (all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) # value of exports by region r at FOB prices #;
Formula (all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) = sum(i,TRAD_COMM, VXW(i,r,t));

Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIW(i,s,t) # value of commodity imports i into s at CIF prices #;
Formula (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIW(i,s,t) = sum(r,REG, VIWS(i,r,s,t));
Coefficient (all,r,REG)(all,t,ALLTIME)
    VIWREGION(r,t) # value of commodity imports by region r at CIF prices #;
Formula (all,r,REG)(all,t,ALLTIME)
    VIWREGION(r,t) = sum(i,TRAD_COMM, VIW(i,r,t));


!<
    A-1. Factor Price Indices
    -------------------------
>!

Variable (orig_level=1.0)(all,i,ENDW_COMM)(all,r,REG)(all,t,ALLTIME)
    pfactreal(i,r,t) # ratio of return to primary factor i to CPI in r #;
Equation REALRETURN
# eq'n defines the real rate of return to primary factor i in region r #
(all,i,ENDW_COMM)(all,s,REG)(all,t,ALLTIME)
    pfactreal(i,s,t) = pm(i,s,t) - ppriv(s,t);
!<
    This equation defines the real rate of return to primary factor i in
    region r (new).
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    VENDWREG(r,t) # value of primary factors, at mkt prices, by region #;
Formula (all,r,REG)(all,t,ALLTIME)
    VENDWREG(r,t) = sum(i,ENDW_COMM, VOM(i,r,t));

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    pfactor(r,t) # market price index of primary factors, by region #;
Equation PRIMFACTPR
# computes % change in price index of primary factors, by region #
(all,r,REG)(all,t,ALLTIME)
    VENDWREG(r,t) * pfactor(r,t) = sum(i,ENDW_COMM, VOM(i,r,t) * pm(i,r,t));

Coefficient (all,t,ALLTIME)
    VENDWWLD(t) # value of primary factors, at mkt prices, worldwide #;
Formula (all,t,ALLTIME)
    VENDWWLD(t) = sum(r,REG, VENDWREG(r,t));
Variable (orig_level=1.0)(all,t,ALLTIME)
    pfactwld(t) # world price index of primary factors #;
Equation PRIMFACTPRWLD
# computes % change in global price index of primary factors #
(all,t,ALLTIME)
    VENDWWLD(t) * pfactwld(t) = sum(r,REG, VENDWREG(r,t) * pfactor(r,t));


!<
    A-2. Regional Terms of Trade
    ----------------------------

    The next three equations correspond to Table 10 of Hertel and Tsigas on
    Regional Terms of Trade.
>!

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    psw(r,t) # index of prices received for tradeables produced in r #;
Equation REGSUPRICE
# estimate change in index of prices received for tradeables i produced in r #
(all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) * psw(r,t)
        = sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s,t) * pfob(i,r,s,t)))
        + sum(m,MARG_COMM, VST(m,r,t) * pm(m,r,t));
!<
    This equation estimates the change in the index of prices received for
    tradeable products produced in r.  (modified from HT 64 to eliminate the
    investment component)
>!

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    pdw(r,t) # index of prices paid for tradeables used in region r #;
Equation REGDEMPRICE
# estimate change in index of prices paid for tradeable products used in r #
(all,r,REG)(all,t,ALLTIME)
    VIWREGION(r,t) * pdw(r,t)
        = sum(i,TRAD_COMM, sum(k,REG, VIWS(i,k,r,t) * pcif(i,k,r,t)));
!<
    This equation estimates the change in the index of prices paid for
    tradeable products used in r.  (modified from HT 65 to eliminate savings,t)
>!

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    tot(r,t) # terms of trade for region r: tot(r,t) = psw(r,t) - pdw(r,t) #;
Equation TOTeq
# terms of trade equation computed as difference in psw and pdw (HT 66) #
(all,r,REG)(all,t,ALLTIME)
    tot(r,t) = psw(r,t) - pdw(r,t);


!<
    A-3. GDP Indices (Value, Price and Quantity)
    --------------------------------------------
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    GDP(r,t) # Gross Domestic Product in region r #;
Formula (all,s,REG)(all,t,ALLTIME)
    GDP(s,t)
        = sum(i,TRAD_COMM, VPA(i,s,t))
        + sum(i,TRAD_COMM, VGA(i,s,t))
        + sum(k,CGDS_COMM, VOA(k,s,t))
        + sum(i,TRAD_COMM, sum(r,REG, VXWD(i,s,r,t)))
        + sum(m,MARG_COMM, VST(m,s,t))
        - sum(i,TRAD_COMM, sum(r,REG, VIWS(i,r,s,t)));
!<
    Gross Domestic Product in region r.  Trade is valued at FOB and CIF
    prices.
>!

Variable (all,r,REG)(all,t,ALLTIME)
    vgdp(r,t) # change in value of GDP #;
Equation VGDP_r
# change in value of GDP (HT 70) #
(all,r,REG)(all,t,ALLTIME)
    GDP(r,t) * vgdp(r,t)
        = sum(i,TRAD_COMM, VGA(i,r,t) * [qg(i,r,t) + pg(i,r,t)])
        + sum(i,TRAD_COMM, VPA(i,r,t) * [qp(i,r,t) + pp(i,r,t)])
        + REGINV(r,t) * [qcgds(r,t) + pcgds(r,t)]
        + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s,t) * [qxs(i,r,s,t)
                 + pfob(i,r,s,t)]))
        + sum(m,MARG_COMM, VST(m,r,t) * [qst(m,r,t) + pm(m,r,t)])
        - sum(i,TRAD_COMM, sum(s,REG,
            VIWS(i,s,r,t) * [qxs(i,s,r,t) + pcif(i,s,r,t)]));

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    pgdp(r,t) # GDP price index #;
Equation PGDP_r
# GDP price index (HT 71) #
(all,r,REG)(all,t,ALLTIME)
    GDP(r,t) * pgdp(r,t)
        = sum(i,TRAD_COMM, VGA(i,r,t) * pg(i,r,t))
        + sum(i,TRAD_COMM, VPA(i,r,t) * pp(i,r,t))
        + REGINV(r,t) * pcgds(r,t)
        + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s,t) * pfob(i,r,s,t)))
        + sum(m,MARG_COMM, VST(m,r,t) * pm(m,r,t))
        - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r,t) * pcif(i,s,r,t)));

Variable (orig_level=GDP)(all,r,REG)(all,t,ALLTIME)
    qgdp(r,t) # GDP quantity index #;
Equation QGDP_r
# GDP quantity index #
(all,r,REG)(all,t,ALLTIME)
    GDP(r,t) * qgdp(r,t)
        = sum(i,TRAD_COMM, VGA(i,r,t) * qg(i,r,t))
        + sum(i,TRAD_COMM, VPA(i,r,t) * qp(i,r,t))
        + REGINV(r,t) * qcgds(r,t)
        + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s,t) * qxs(i,r,s,t)))
        + sum(m,MARG_COMM, VST(m,r,t) * qst(m,r,t))
        - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r,t) * qxs(i,s,r,t)));
!<
    modified from HT 72 for use with AnalyzeGE
>!

Variable (all,i,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    compvalad(i,r,t) # composition of value added for good i and region r #;
Equation COMPVALADEQ
# track change in composition of value added #
(all,i,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    compvalad(i,r,t) = qo(i,r,t) - qgdp(r,t);


!<
    A-4. Aggregate Trade Indices (Value, Price and Quantity)
    --------------------------------------------------------
>!


!<
    Common Variables and Coefficients
    Value Indices for Aggregate Trade
    Price Indices for Aggregate Trade
    Quantity Indices for Aggregate Trade
>!


!<
    Common Variables and Coefficients
    .................................

    only used in this Aggregate Trade Indices section
>!

Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pxw(i,r,t) # aggregate exports price index of i from region r #;

Coefficient (all,i,TRAD_COMM)(all,t,ALLTIME)
    VXWCOMMOD(i,t) # value of world exports by commodity i at FOB prices #;
Formula (all,i,TRAD_COMM)(all,t,ALLTIME)
    VXWCOMMOD(i,t) = sum(r,REG, VXW(i,r,t));

Coefficient (all,t,ALLTIME)
    VXWLD(t) # value of commodity exports, FOB, globally #;
Formula
(all,t,ALLTIME)
    VXWLD(t) = sum(r,REG, VXWREGION(r,t));

Coefficient (all,i,TRAD_COMM)(all,t,ALLTIME)
    VIWCOMMOD(i,t) # global value of commodity imports, CIF, by commodity #;
Formula (all,i,TRAD_COMM)(all,t,ALLTIME)
    VIWCOMMOD(i,t) = sum(r,REG, VIW(i,r,t));

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    PW_PM(i,r,t) # ratio of world to domestic prices #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    PW_PM(i,r,t) = sum(s,REG, VXWD(i,r,s,t)) / sum(s,REG, VXMD(i,r,s,t));
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VOW(i,r,t) #value of output in r at FOB including transportation services#;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VOW(m,r,t) = VDM(m,r,t) * PW_PM(m,r,t)
                    + sum(s,REG, VXWD(m,r,s,t)) + VST(m,r,t);
Formula (all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    VOW(i,r,t) = VDM(i,r,t) * PW_PM(i,r,t) + sum(s,REG, VXWD(i,r,s,t));

Coefficient (all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOW(i,t) # value of world supply at world prices for i #;
Formula (all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOW(i,t) = sum(r,REG, VOW(i,r,t));

Coefficient (all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOU(i,t) # value of world output of i at user prices #;
Formula (all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOU(i,t) = sum(s,REG, [VPA(i,s,t) + VGA(i,s,t)]
                     + sum(j,PROD_COMM, VFA(i,j,s,t)));


!<
    Value Indices for Aggregate Trade
    .................................
>!

Equation VREGEX_ir_MARG
# the change in FOB value of exports of m from r #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(m,r,t) * vxwfob(m,r,t)
        = sum(s,REG, VXWD(m,r,s,t) * [qxs(m,r,s,t) + pfob(m,r,s,t)])
        + VST(m,r,t) * [qst(m,r,t) + pm(m,r,t)];
Equation VREGEX_ir_NMRG
# the change in FOB value of exports of commodity i from r (HT 73) #
(all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(i,r,t) * vxwfob(i,r,t)
        = sum(s,REG, VXWD(i,r,s,t) * [qxs(i,r,s,t) + pfob(i,r,s,t)]);

Equation VREGEX_r
# computes % change in value of merchandise exports, by region (HT 75) #
(all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) * vxwreg(r,t) = sum(i,TRAD_COMM, VXW(i,r,t) * vxwfob(i,r,t));

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    vxwcom(i,t) # value of global merchandise exports by commodity #;
Equation VWLDEX_i
# computes % change in FOB value of global exports, by commodity (HT 77) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VXWCOMMOD(i,t) * vxwcom(i,t) = sum(r,REG, VXW(i,r,t) * vxwfob(i,r,t));

Variable (all,t,ALLTIME)
    vxwwld(t) # value of world trade #;
Equation VWLDEX
# computes % change in value of global exports (HT 79) #
(all,t,ALLTIME)
    VXWLD(t) * vxwwld(t) = sum(r,REG, VXWREGION(r,t) * vxwreg(r,t));

Equation VREGIM_is
# the change in CIF value of imports of commodity i into s (HT 74) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIW(i,s,t) * viwcif(i,s,t)
        = sum(r,REG, VIWS(i,r,s,t) * [pcif(i,r,s,t) + qxs(i,r,s,t)]);

Equation VREGIM_s
# computes % change in value of imports, CIF basis, by region (HT 76) #
(all,s,REG)(all,t,ALLTIME)
    VIWREGION(s,t) * viwreg(s,t) = sum(i,TRAD_COMM, VIW(i,s,t) * viwcif(i,s,t));

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    viwcom(i,t) # value of global merchandise imports i, at world prices #;
Equation VWLDIM_i
# computes % change in value of global imports, by commodity (HT 78) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VIWCOMMOD(i,t) * viwcom(i,t) = sum(s,REG, VIW(i,s,t) * viwcif(i,s,t));

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    valuew(i,t) # value of world supply of good i #;
Equation VWLDOUT
# change in value of world output of comm. i at FOB prices (HT 80) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOW(i,t) * valuew(i,t) = sum(r,REG, VOW(i,r,t) * [pxw(i,r,t) + qo(i,r,t)]);

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    valuewu(i,t) # value of world supply of good i at user prices #;
Equation VWLDOUTUSE
# change in value of world output of commodity i at user prices #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOU(i,t) * valuewu(i,t)
        = sum(s,REG, VPA(i,s,t) * [pp(i,s,t) + qp(i,s,t)]
        + VGA(i,s,t) * [pg(i,s,t) + qg(i,s,t)]
        + sum(j,PROD_COMM, VFA(i,j,s,t) * [pf(i,j,s,t) + qf(i,j,s,t)]));


!<
    Price Indices for Aggregate Trade
    .................................
>!

Equation PREGEX_ir_MARG
# change in FOB price index of exports of m from r #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(m,r,t) * pxw(m,r,t)
        = sum(s,REG, VXWD(m,r,s,t) * pfob(m,r,s,t)) + VST(m,r,t) * pm(m,r,t);

Equation PREGEX_ir_NMRG
# change in FOB price index of exports of commodity i from r (HT 81) #
(all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(i,r,t) * pxw(i,r,t) = sum(s,REG, VXWD(i,r,s,t) * pfob(i,r,s,t));

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    pxwreg(r,t) # price index of merchandise exports, by region #;
Equation PREGEX_r
# computes % change in price index of exports, by region (HT 83) #
(all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) * pxwreg(r,t) = sum(i,TRAD_COMM, VXW(i,r,t) * pxw(i,r,t));

Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,t,ALLTIME)
    pxwcom(i,t) # price index of global merchandise exports by commodity #;
Equation PWLDEX_i
# computes % change in price index of exports, by commodity (HT 85) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VXWCOMMOD(i,t) * pxwcom(i,t) = sum(r,REG, VXW(i,r,t) * pxw(i,r,t));

Variable (orig_level=1.0)(all,t,ALLTIME)
    pxwwld(t) # price index of world trade #;
Equation PWLDEX
# computes % change in price index of global exports (HT 87) #
(all,t,ALLTIME)
    VXWLD(t) * pxwwld(t) = sum(r,REG, VXWREGION(r,t) * pxwreg(r,t));

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    piw(i,r,t) # world price of composite import i in region r #;
Equation PREGIM_is
# change in CIF price index of imports of commodity i into s (HT 82) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIW(i,s,t) * piw(i,s,t) = sum(r,REG, VIWS(i,r,s,t) * pcif(i,r,s,t));

Variable (orig_level=1.0)(all,r,REG)(all,t,ALLTIME)
    piwreg(r,t) # price index of merchandise imports, by region #;
Equation PREGIM_s
# computes % change in price index of imports, by region (HT 84) #
(all,s,REG)(all,t,ALLTIME)
    VIWREGION(s,t) * piwreg(s,t) = sum(i,TRAD_COMM, VIW(i,s,t) * piw(i,s,t));

Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,t,ALLTIME)
    piwcom(i,t) # price index of global merchandise imports by commodity #;
Equation PWLDIM_i
# computes % change in price index of imports, by commodity (HT 86) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VIWCOMMOD(i,t) * piwcom(i,t) = sum(s,REG, VIW(i,s,t) * piw(i,s,t));

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    pw(i,t) # world price index for total good i supplies #;
Equation PWLDOUT
# change in index of world prices, FOB, for total production of i (HT 88) #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOW(i,t) * pw(i,t) = sum(r,REG, VOW(i,r,t) * pxw(i,r,t));

Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,t,ALLTIME)
    pwu(i,t) # world price index for total good i supplies at user prices #;
Equation PWLDUSE
# change in index of user prices for deflating world production of i #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOU(i,t) * pwu(i,t)
        = sum(s,REG, VPA(i,s,t) * pp(i,s,t)
        + VGA(i,s,t) * pg(i,s,t)
        + sum(j,PROD_COMM, VFA(i,j,s,t) * pf(i,j,s,t)));


!<
    Quantity Indices for Aggregate Trade
    ....................................
>!

Variable (orig_level=VXW)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qxw(i,r,t) # aggregate exports of i from region r, FOB weights #;
Equation QREGEX_ir_MARG
# change in volume of exports of margin commodity m from r #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(m,r,t) * qxw(m,r,t)
        = sum(s,REG, VXWD(m,r,s,t) * qxs(m,r,s,t)) + VST(m,r,t) * qst(m,r,t);
Equation QREGEX_ir_NMRG
# change in volume of exports of non-margin commodity i from r #
(all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    VXW(i,r,t) * qxw(i,r,t) = sum(s,REG, VXWD(i,r,s,t) * qxs(i,r,s,t));
!<
    modified from HT 89 for use with AnalyzeGE
>!

Variable (orig_level=VXWREGION)(all,r,REG)(all,t,ALLTIME)
    qxwreg(r,t) # volume of merchandise exports, by region #;
Equation QREGEX_r
# computes % change in quantity index of exports, by region #
(all,r,REG)(all,t,ALLTIME)
    VXWREGION(r,t) * qxwreg(r,t) = sum(i,TRAD_COMM, VXW(i,r,t) * qxw(i,r,t));
!<
    modified from HT 91 for use with AnalyzeGE
>!

Variable (orig_level=VXWCOMMOD)(all,i,TRAD_COMM)(all,t,ALLTIME)
    qxwcom(i,t) # volume of global merchandise exports by commodity #;
Equation QWLDEX_i
# computes % change in quantity index of exports, by commodity #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VXWCOMMOD(i,t) * qxwcom(i,t) = sum(r,REG, VXW(i,r,t) * qxw(i,r,t));
!<
    modified from HT 93 for use with AnalyzeGE
>!

Variable (orig_level=VXWLD)(all,t,ALLTIME)
    qxwwld(t) # volume of world trade #;
Equation QWLDEX
# computes % change in quantity index of global exports #
(all,t,ALLTIME)
    VXWLD(t) * qxwwld(t) = sum(r,REG, VXWREGION(r,t) * qxwreg(r,t));
!<
    modified from HT 95 for use with AnalyzeGE
>!

Variable (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    qiw(i,s,t) # aggregate imports of i into region s, CIF weights #;
Equation QREGIM_is
# change in volume of imports of commodity i into s #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIW(i,s,t) * qiw(i,s,t) = sum(r,REG, VIWS(i,r,s,t) * qxs(i,r,s,t));
!<
    modified from HT 90 for use with AnalyzeGE
>!

Variable (orig_level=VIWREGION)(all,r,REG)(all,t,ALLTIME)
    qiwreg(r,t) # volume of merchandise imports, by region #;
Equation QREGIM_s
# computes % change in quantity index of imports, by region #
(all,s,REG)(all,t,ALLTIME)
    VIWREGION(s,t) * qiwreg(s,t) = sum(i,TRAD_COMM, VIW(i,s,t) * qiw(i,s,t));
!<
    modified from HT 92 for use with AnalyzeGE
>!

Variable (orig_level=VIWCOMMOD)(all,i,TRAD_COMM)(all,t,ALLTIME)
    qiwcom(i,t) # volume of global merchandise imports by commodity #;
Equation QWLDIM_i
# computes % change in quantity index of imports, by commodity #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VIWCOMMOD(i,t) * qiwcom(i,t) = sum(s,REG, VIW(i,s,t) * qiw(i,s,t));
!<
    modified from HT 94 for use with AnalyzeGE
>!

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    qow(i,t) # quantity index for world supply of good i #;
Equation QWLDOUT
# change in index of world production of i #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOW(i,t) * qow(i,t) = sum(r,REG, VOW(i,r,t) * qo(i,r,t));
!<
    modified from HT 96 for use with AnalyzeGE
>!

Variable (orig_level=VWOU)(all,i,TRAD_COMM)(all,t,ALLTIME)
    qowu(i,t) # quantity index for world supply of good i at user prices #;
Equation QWLDOUTU
# change in index of world production of i evaluated at user prices #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    VWOU(i,t) * qowu(i,t)
        = sum(s,REG, VPA(i,s,t) * qp(i,s,t)
        + VGA(i,s,t) * qg(i,s,t)
        + sum(j,PROD_COMM, VFA(i,j,s,t) * qf(i,j,s,t)));


!<
    A-5. Trade Balance Indices
    --------------------------
>!

Variable (change)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    DTBALi(i,r,t) # change in trade balance by i and by r, $ US million #;
!<
    A positive value indicates that the change in exports exceeds the change
    in imports.
>!
Equation TRADEBAL_i
# computes change in trade balance by commodity and by region (HT 97) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    DTBALi(i,r,t)
        = [VXW(i,r,t) / 100] * vxwfob(i,r,t)
          - [VIW(i,r,t) / 100] * viwcif(i,r,t);

Variable (change)(all,r,REG)(all,t,ALLTIME)
    DTBAL(r,t) # change in trade balance X - M, $ US million #;
Equation TRADEBALANCE
# computes change in trade balance (X - M), by region (HT 98) #
(all,r,REG)(all,t,ALLTIME)
    DTBAL(r,t)
        = [VXWREGION(r,t) / 100] * vxwreg(r,t)
          - [VIWREGION(r,t) / 100] * viwreg(r,t);

!<
    In order to maintain homogeneity in the presence of a fixed trade balance,
    it is useful to have a nominal variable which this is measured against.
    The next equation provides this, and we recommend users fix DTBALR instead
    of fixing DTBAL in future simulations.  The strategy is the same one used
    above for taxes.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    TBAL(r,t) # trade balance for region r #;
Formula (all,r,REG)(all,t,ALLTIME)
    TBAL(r,t) = VXWREGION(r,t) - VIWREGION(r,t);

Variable (change)(all,r,REG)(all,t,ALLTIME)
    DTBALR(r,t) # change in ratio of trade balance to regional income #;
Equation DTBALRATIO
# change in ratio of trade balance to regional income #
(all,r,REG)(all,t,ALLTIME)
    100 * INCOME(r,t) * DTBALR(r,t) = 100 * DTBAL(r,t) - TBAL(r,t) * y(r,t);


!<
    -----------------------
    B. Equivalent Variation
    -----------------------

    B-0. Appendix-Specific Variables and Coefficients
    B-1. Government Consumption Shadow Demand System
    B-2. Private Consumption Shadow Demand System
    B-3. Regional Household Shadow Demand System
    B-4. Equivalent Variation

    This appendix calculates equivalent variation "EV" and world equivalent
    variation, "WEV", by determining the income "yev" that would be required
    to achieve the current actual utility level "u" in a shadow demand system
    in which prices are fixed.

    Section B-2 calculates the utility elasticity of private consumption
    expenditure, "ueprivev", within a shadow demand system for private
    consumption, for use in section B-3.  B-3 calculates private consumption
    expenditure "ypev" for use in B-2, and regional income "yev" for use in
    B-4, within a shadow demand system for the regional household.  B-4
    calculates "EV" and "WEV".
>!


!<
    B-0. Appendix-Specific Variables and Coefficients
    -------------------------------------------------

    only used in this Equivalent Variation section of the Summary Indices
    appendix
>!

Variable (all,r,REG)(all,t,ALLTIME)
    uelasev(r,t) # elasticity of cost of utility wrt utility, for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    ueprivev(r,t) # utility elasticity of private consn expenditure, for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    ugev(r,t) # per capita utility from gov't expend., for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    upev(r,t) # per capita utility from private expend., for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    qsaveev(r,t) # total quantity of savings demanded, for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    yev(r,t) # regional household income in region r, for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    ypev(r,t) # private consumption expenditure in region r, for EV calc. #;
Variable (all,r,REG)(all,t,ALLTIME)
    ygev(r,t) # government consumption expenditure in region r, for EV calc. #;

Coefficient (all,r,REG)(all,t,ALLTIME)
    INCOMEEV(r,t) # regional income, for EV calc. #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    INCOMEEV(r,t) = INCOME(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    INCOMEEV(r,t) = yev(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILPRIVEV(r,t) # utility from private consumption, for EV calcs #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILPRIVEV(r,t) = UTILPRIV(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    UTILPRIVEV(r,t) = upev(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILGOVEV(r,t) # utility from private consumption, for EV calcs #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILGOVEV(r,t) = UTILGOV(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    UTILGOVEV(r,t) = ugev(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILSAVEEV(r,t) # utility from private consumption, for EV calcs #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILSAVEEV(r,t) = UTILSAVE(r,t);
Update (change)(all,r,REG)(all,t,ALLTIME)
    UTILSAVEEV(r,t) = [[qsaveev(r,t) - pop(r,t)] / 100] * UTILSAVEEV(r,t);


!<
    B-1. Government Consumption Shadow Demand System
    ------------------------------------------------
>!

Equation GOVUSHD
# utility from government consumption in r #
(all,r,REG)(all,t,ALLTIME)
    ygev(r,t) - pop(r,t) = ugev(r,t);


!<
    B-2. Private Consumption Shadow Demand System
    ---------------------------------------------
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qpev(i,r,t) # private hhld demand for commodity i in region r, for EV calc. #;

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VPAEV(i,r,t) # private hhld expend. on i in r valued at agent's prices, for EV calc. #;
Formula (initial)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VPAEV(i,r,t) = VPA(i,r,t);
Update (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VPAEV(i,r,t) = qpev(i,r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    VPAREGEV(r,t) # private consumption expenditure in region r, for EV calc. #;
Formula (all,r,REG)(all,t,ALLTIME)
    VPAREGEV(r,t) = sum(i,TRAD_COMM, VPAEV(i,r,t));
!<
    VPAREGEV should agree with PRIVEXPEV.
>!

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    CONSHREV(i,r,t) # share of private hhld consn devoted to good i in r, for EV calc. #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    CONSHREV(i,r,t) = VPAEV(i,r,t) / VPAREGEV(r,t);
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EYEV(i,r,t) # expend. elast. of private hhld demand for i in r, for EV calc. #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EYEV(i,r,t)
        = [1.0 / sum(n,TRAD_COMM, CONSHREV(n,r,t) * INCPAR(n,r,t))]
        * [INCPAR(i,r,t) * [1.0 - ALPHA(i,r,t)]
            + sum(n,TRAD_COMM, CONSHREV(n,r,t) * INCPAR(n,r,t) * ALPHA(n,r,t))]
        + ALPHA(i,r,t)
        - sum(n,TRAD_COMM, CONSHREV(n,r,t) * ALPHA(n,r,t));

Equation PRIVDMNDSEV
    # private hhld demands for composite commodities, for EV calc. #
    (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qpev(i,r,t) - pop(r,t) = EYEV(i,r,t) * [ypev(r,t) - pop(r,t)];
!<
    Prices are held constant for the EV calculation and so do not appear here.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    UELASPRIVEV(r,t) # elast. of cost wrt utility from private consn, for EV calc. #;
Formula (all,r,REG)(all,t,ALLTIME)
    UELASPRIVEV(r,t) = sum(i,TRAD_COMM, CONSHREV(i,r,t) * INCPAR(i,r,t));

Equation PRIVATEUEV
# computation of utility from private consumption in r (HT 45) #
(all,r,REG)(all,t,ALLTIME)
    ypev(r,t) - pop(r,t) = UELASPRIVEV(r,t) * upev(r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    XWCONSHREV(i,r,t) # expansion-parameter-weighted consumption share, for EV calc. #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    XWCONSHREV(i,r,t) = CONSHREV(i,r,t) * INCPAR(i,r,t) / UELASPRIVEV(r,t);

Equation UTILELASPRIVEV
    # elasticity of cost wrt utility from private consn, for EV calc. #
    (all,r,REG)(all,t,ALLTIME)
    ueprivev(r,t)
        = sum(i,TRAD_COMM, XWCONSHREV(i,r,t) * [qpev(i,r,t) - ypev(r,t)]);
!<
    Prices are held constant for the EV calculation and so do not appear here.
>!


!<

    B-3. Regional Household Shadow Demand System
    --------------------------------------------
>!

Variable (all,r,REG)(all,t,ALLTIME)
    ysaveev(r,t) # NET savings expenditure, for EV calc. #;

Coefficient (all,r,REG)(all,t,ALLTIME)
    PRIVEXPEV(r,t) # private consumption expenditure in region r, for EV calc. #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    PRIVEXPEV(r,t) = PRIVEXP(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    PRIVEXPEV(r,t) = ypev(r,t);
!<
    PRIVEXPEV should agree with VPAREGEV.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    GOVEXPEV(r,t) # government consumption expenditure in region r, for EV calc. #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    GOVEXPEV(r,t) = GOVEXP(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    GOVEXPEV(r,t) = ygev(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    SAVEEV(r,t) # saving in region r, for EV calc. #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    SAVEEV(r,t) = SAVE(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    SAVEEV(r,t) = ysaveev(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRPRIVEV(r,t) # private expenditure share in regional income, for EV calc. #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRPRIVEV(r,t) = PRIVEXPEV(r,t) / INCOMEEV(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRGOVEV(r,t) # government expenditure share in regional income, for EV calc. #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRGOVEV(r,t) = GOVEXPEV(r,t) / INCOMEEV(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRSAVEEV(r,t) # saving share in regional income, for EV calc. #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRSAVEEV(r,t) = SAVEEV(r,t) / INCOMEEV(r,t);

Variable (all,r,REG)(all,t,ALLTIME)
    dpavev(r,t) # average distribution parameter shift, for EV calc. #;
Equation DPARAVEV
# average distribution parameter shift, for EV calc. #
    (all,r,REG)(all,t,ALLTIME)
    dpavev(r,t)
        = XSHRPRIVEV(r,t) * dppriv(r,t)
        + XSHRGOVEV(r,t) * dpgov(r,t)
        + XSHRSAVEEV(r,t) * dpsave(r,t);

Equation UTILITELASTICEV
    # elasticity of cost of utility wrt utility, for EV calc. #
    (all,r,REG)(all,t,ALLTIME)
    uelasev(r,t) = XSHRPRIV(r,t) * ueprivev(r,t) - dpavev(r,t);

Equation PCONSEXPEV
# private consumption expenditure, for EV calc. #
    (all,r,REG)(all,t,ALLTIME)
    ypev(r,t) - yev(r,t) = -[ueprivev(r,t) - uelasev(r,t)] + dppriv(r,t);

Equation GOVCONSEXPEV
# government consumption expenditure #
(all,r,REG)(all,t,ALLTIME)
    ygev(r,t) - yev(r,t) = uelasev(r,t) + dpgov(r,t);

Equation SAVINGEV
# saving #
(all,r,REG)(all,t,ALLTIME)
    ysaveev(r,t) - yev(r,t) = uelasev(r,t) + dpsave(r,t);

Equation SAVEUEV
# saving #
(all,r,REG)(all,t,ALLTIME)
    qsaveev(r,t) = ysaveev(r,t);

!<
    Note that because psave doesn't change, qsaveev moves with ysaveev.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILELASEV(r,t) # elasticity of cost of utility wrt utility, for EV calc. #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILELASEV(r,t) = UTILELAS(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    UTILELASEV(r,t) = uelasev(r,t);

Equation INCOME_EQUIV
# equivalent income #
(all,r,REG)(all,t,ALLTIME)
    u(r,t)
        = au(r,t)
        + DPARPRIV(r,t) * loge(UTILPRIVEV(r,t)) * dppriv(r,t)
        + DPARGOV(r,t) * loge(UTILGOVEV(r,t)) * dpgov(r,t)
        + DPARSAVE(r,t) * loge(UTILSAVEEV(r,t)) * dpsave(r,t)
        + [1.0 / UTILELASEV(r,t)] * [yev(r,t) - pop(r,t)];


!<
    B-4. Equivalent Variation
    -------------------------
>!

Variable (change)(all,r,REG)(all,t,ALLTIME)
    EV(r,t) # equivalent variation, $ US million #;
Equation EVREG
# regional EV (HT 67) #
(all,r,REG)(all,t,ALLTIME)
    EV(r,t) = [INCOMEEV(r,t) / 100] * yev(r,t);

Variable (change)(all,t,ALLTIME)
    WEV(t) # equivalent variation for the world #;
Equation EVWLD
# EV for the world (HT 68) #
(all,t,ALLTIME)
    WEV(t) = sum(r,REG, EV(r,t));


!<
    ------------------------
    C. Welfare Decomposition
    ------------------------
    See GTAP Technical Paper No. 5 for derivation and interpretation.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    EVSCALFACT(r,t) # equivalent variation scaling factor #;
Formula (all,r,REG)(all,t,ALLTIME)
    EVSCALFACT(r,t) =
         [UTILELASEV(r,t) / UTILELAS(r,t)] * [INCOMEEV(r,t) / INCOME(r,t)];

Coefficient (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    VTMD(m,s,t) # aggregate value of svce m in shipments to s #;
Formula (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    VTMD(m,s,t) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s,t)));

Variable (linear,change)(all,r,REG)(all,t,ALLTIME)
    EV_ALT(r,t) # regional EV computed in alternative way #;
Equation EV_DECOMPOSITION
# decomposition of Equivalent Variation #
(all,r,REG)(all,t,ALLTIME)
    EV_ALT(r,t)
        = -[0.01 * UTILELASEV(r,t) * INCOMEEV(r,t)]
        * [DPARPRIV(r,t) * loge(UTILPRIVEV(r,t) / UTILPRIV(r,t)) * dppriv(r,t)
            + DPARGOV(r,t) * loge(UTILGOVEV(r,t) / UTILGOV(r,t)) * dpgov(r,t)
            + DPARSAVE(r,t)*loge(UTILSAVEEV(r,t) / UTILSAVE(r,t)) * dpsave(r,t)]
        + [0.01 * EVSCALFACT(r,t)]
        * [sum(i,NSAV_COMM, PTAX(i,r,t) * [qo(i,r,t) - pop(r,t)])
            + sum(i,ENDW_COMM, sum(j,PROD_COMM,
                ETAX(i,j,r,t) * [qfe(i,j,r,t) - pop(r,t)]))
            + sum(i,TRAD_COMM, sum(j,PROD_COMM,
                IFTAX(i,j,r,t) * [qfm(i,j,r,t) - pop(r,t)]))
            + sum(i,TRAD_COMM, sum(j,PROD_COMM,
                DFTAX(i,j,r,t) * [qfd(i,j,r,t) - pop(r,t)]))
            + sum(i,TRAD_COMM, IPTAX(i,r,t) * [qpm(i,r,t) - pop(r,t)])
            + sum(i,TRAD_COMM, DPTAX(i,r,t) * [qpd(i,r,t) - pop(r,t)])
            + sum(i,TRAD_COMM, IGTAX(i,r,t) * [qgm(i,r,t) - pop(r,t)])
            + sum(i,TRAD_COMM, DGTAX(i,r,t) * [qgd(i,r,t) - pop(r,t)])
            + sum(i,TRAD_COMM,sum(s,REG,XTAXD(i,r,s,t)*[qxs(i,r,s,t)-pop(r,t)]))
            + sum(i,TRAD_COMM,sum(s,REG, MTAX(i,s,r,t)*[qxs(i,s,r,t)-pop(r,t)]))
            + sum(i,ENDW_COMM, VOA(i,r,t) * [qo(i,r,t) - pop(r,t)])
            - VDEP(r,t) * [qo("capital",r,t) - pop(r,t)]
            + sum(i,PROD_COMM, VOA(i,r,t) * ao(i,r,t))
            + sum(j,PROD_COMM, VVA(j,r,t) * ava(j,r,t))
            + sum(j,PROD_COMM, sum(i,ENDW_COMM, VFA(i,j,r,t) * afe(i,j,r,t)))
            + sum(j,PROD_COMM, sum(i,TRAD_COMM, VFA(i,j,r,t) * af(i,j,r,t)))
            + sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
                VTMFSD(m,i,s,r,t) * atmfsd(m,i,s,r,t))))
            + sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r,t) * ams(i,s,r,t)))
            + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s,t) * pfob(i,r,s,t)))
            + sum(m,MARG_COMM, VST(m,r,t) * pm(m,r,t))
            + NETINV(r,t) * pcgds(r,t)
            - sum(i,TRAD_COMM, sum(s,REG, VXWD(i,s,r,t) * pfob(i,s,r,t)))
            - sum(m,MARG_COMM, VTMD(m,r,t) * pt(m,t))
            - SAVE(r,t) * psave(r,t)]
        + 0.01 * INCOMEEV(r,t) * pop(r,t);

