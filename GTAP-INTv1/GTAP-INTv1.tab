!<
    1-0. Module-Specific Variables
    ------------------------------

    only used in this Government Consumption module
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pg(i,r,t) # government consumption price for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qg(i,r,t) # government hhld demand for commodity i in region r #;


!<
    1-1. Demands for Composite Goods
    --------------------------------
>!

Equation GPRICEINDEX
# definition of price index for aggregate gov't purchases (HT 40) #
(all,r,REG)(all,t,ALLTIME)
    pgov(r,t) = sum(i,TRAD_COMM, [VGA(i,r,t) / GOVEXP(r,t)] * pg(i,r,t));

Equation GOVDMNDS
# government consumption demands for composite commodities (HT 41) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qg(i,r,t) - pop(r,t) = ug(r,t) - [pg(i,r,t) - pgov(r,t)];

Equation GOVU
# utility from government consumption in r #
(all,r,REG)(all,t,ALLTIME)
    yg(r,t) - pop(r,t) = pgov(r,t) + ug(r,t);


!<
    1-2. Composite Tradeables
    -------------------------
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tgd(i,r,t) # tax on domestic i purchased by government hhld in r #;

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tgm(i,r,t) # tax on imported i purchased by gov't hhld in r #;

Equation GHHDPRICE
# eq'n links domestic market and government consumption prices (HT 19) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pgd(i,r,t) = tgd(i,r,t) + pm(i,r,t);

Equation GHHIPRICES
# eq'n links domestic market and government consumption prices (HT 22) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pgm(i,r,t) = tgm(i,r,t) + pim(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    GMSHR(i,s,t) # share of imports for gov't hhld at agent's prices #;
Formula (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    GMSHR(i,s,t) = VIGA(i,s,t) / VGA(i,s,t);

Equation GCOMPRICE
# government consumption price for composite commodities (HT 42) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    pg(i,s,t) =  GMSHR(i,s,t) * pgm(i,s,t) + [1 - GMSHR(i,s,t)] * pgd(i,s,t);

Equation GHHLDAGRIMP
# government consumption demand for aggregate imports (HT 43) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    qgm(i,s,t) = qg(i,s,t) + ESUBD(i,t) * [pg(i,s,t) - pgm(i,s,t)];

Equation GHHLDDOM
# government consumption demand for domestic goods (HT 44) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    qgd(i,s,t) = qg(i,s,t) + ESUBD(i,t) * [pg(i,s,t) - pgd(i,s,t)];

Equation TGCRATIO
# change in ratio of government consumption tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrgc(r,t) + TGC(r,t) * y(r,t)
        = sum(i,TRAD_COMM,
            VDGA(i,r,t) * tgd(i,r,t) + DGTAX(i,r,t) * [pm(i,r,t) + qgd(i,r,t)])
        + sum(i,TRAD_COMM,
            VIGA(i,r,t)* tgm(i,r,t) + IGTAX(i,r,t) * [pim(i,r,t) + qgm(i,r,t)]);


!<
    -----------------------------
    2. Private Consumption Module
    -----------------------------

    2-0. Module-Specific Variables
    2-1. Utility from Private Consumption
    2-2. Allen Partials, Price and Income Elasticities, Composite Demand
    2-3. Composite Tradeables
>!


!<
    2-0. Module-Specific Variables
    ------------------------------

    only used in this Private Consumption module
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pp(i,r,t) # private consumption price for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qp(i,r,t) # private hhld demand for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qptaste(i,r,t) # private hhld demand shift for commodity i in region r #;


!<
    2-1. Utility from Private Consumption
    -------------------------------------
>!

Equation PHHLDINDEX
# price index for private consumption expenditure #
(all,r,REG)(all,t,ALLTIME)
    ppriv(r,t) = sum(i,TRAD_COMM, CONSHR(i,r,t) * pp(i,r,t));

Equation PRIVATEU
# computation of utility from private consumption in r (HT 45) #
(all,r,REG)(all,t,ALLTIME)
    yp(r,t) - pop(r,t) = ppriv(r,t) + UELASPRIV(r,t) * up(r,t);
!<
    This equation determines private consumption utility for a representative
    household in region r, based on the per capita private expenditure
    function.  (HT 45)
>!

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    XWCONSHR(i,r,t) # expansion-parameter-weighted consumption share #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    XWCONSHR(i,r,t) = CONSHR(i,r,t) * INCPAR(i,r,t) / UELASPRIV(r,t);

Equation UTILELASPRIV
# elasticity of expenditure wrt utility from private consumption #
(all,r,REG)(all,t,ALLTIME)
    uepriv(r,t)
        = sum(i,TRAD_COMM, XWCONSHR(i,r,t) * [pp(i,r,t) + qp(i,r,t) - yp(r,t)]);


!<
    2-2. Allen Partials, Price and Income Elasticities, Composite Demand
    --------------------------------------------------------------------
>!

Coefficient (parameter)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SUBPAR(i,r,t) # substitution parameter in CDE minimum expenditure function #;
Read
    SUBPAR from file GTAPPARM header "SUBP";

!<
    (HT F1 - F3)
>!
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    ALPHA(i,r,t) # 1 - sub. parameter in the CDE minimum expenditure function #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    ALPHA(i,r,t) = 1 - SUBPAR(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,k,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    APE(i,k,r,t) # Allen partial elst. of sub. between composite i and k in r #;
Formula (all,i,TRAD_COMM)(all,k,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    APE(i,k,r,t) = ALPHA(i,r,t) + ALPHA(k,r,t)
                   - sum(n,TRAD_COMM, CONSHR(n,r,t) * ALPHA(n,r,t));
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    APE(i,i,r,t)
        = 2.0 * ALPHA(i,r,t)
        - sum(n,TRAD_COMM, CONSHR(n,r,t) * ALPHA(n,r,t))
        - ALPHA(i,r,t) / CONSHR(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EY(i,r,t) # income elast. of private hhld demand for i in r (HT F4) #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EY(i,r,t)
        =   [1.0 / sum(n,TRAD_COMM, CONSHR(n,r,t) * INCPAR(n,r,t))]
            * [INCPAR(i,r,t) * [1.0 - ALPHA(i,r,t)]
               + sum(n,TRAD_COMM, CONSHR(n,r,t) * INCPAR(n,r,t) * ALPHA(n,r,t))]
        + [ALPHA(i,r,t) - sum(n,TRAD_COMM, CONSHR(n,r,t) * ALPHA(n,r,t))];

Coefficient (all,i,TRAD_COMM)(all,k,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EP(i,k,r,t) # uncomp. elast. of private hhld demand for i wrt price of k in r (HT F5) #;
Formula (all,i,TRAD_COMM)(all,k,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EP(i,k,r,t) = 0;
Formula (all,i,TRAD_COMM)(all,k,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    EP(i,k,r,t) = [APE(i,k,r,t) - EY(i,r,t)] * CONSHR(k,r,t);

Equation PRIVDMNDS
# private consumption demands for composite commodities (HT 46) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qp(i,r,t) - pop(r,t) - qptaste(i,r,t)
        = sum(k,TRAD_COMM, EP(i,k,r,t) * pp(k,r,t))
          + EY(i,r,t) * [yp(r,t) - pop(r,t)];
!<
    Private consumption demands for composite commodities.  Demand system is
    on a per capita basis.  Here, yp(r,t) - pop(r,t) is % change in per capita
    income.  (HT 46)
>!


!<
    2-3. Composite Tradeables
    -------------------------
>!

Variable (all,r,REG)(all,t,ALLTIME)
    tp(r,t) # comm.-, source-gen. shift in tax on private cons. #;
!<
  The variable tp(r,t) can be swapped with del_ttaxr(r,t) in order to generate a
  tax replacement scenario, whereby taxes remain a constant share of
  national income.
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tpd(i,r,t) # comm.-, source-spec. shift in tax on private cons. of dom. #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    atpd(i,r,t) # power of tax on domestic i purchased by private hhld in r #;
Equation TPDSHIFT
# permits uniform consumption tax change #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    atpd(i,r,t) = tpd(i,r,t) + tp(r,t);

Equation PHHDPRICE
# eq'n links domestic market and private consumption prices (HT 18) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    ppd(i,r,t) = atpd(i,r,t) + pm(i,r,t);

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tpm(i,r,t) # comm.-, source-spec. shift in tax on private cons. of imp. #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    atpm(i,r,t) # power of tax on imported i purchased by private hhld in r #;
Equation TPMSHIFT
# permits uniform consumption tax change #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    atpm(i,r,t) = tpm(i,r,t) + tp(r,t);

Equation PHHIPRICES
# eq'n links domestic market and private consumption prices (HT 21) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    ppm(i,r,t) = atpm(i,r,t) + pim(i,r,t);

Equation TPCRATIO
# change in ratio of private consumption tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrpc(r,t) + TPC(r,t) * y(r,t)
        = sum(i,TRAD_COMM,
            VDPA(i,r,t) * atpd(i,r,t) + DPTAX(i,r,t) * [pm(i,r,t) + qpd(i,r,t)])
        + sum(i,TRAD_COMM,
            VIPA(i,r,t)* atpm(i,r,t) + IPTAX(i,r,t)* [pim(i,r,t) + qpm(i,r,t)]);

Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    PMSHR(i,s,t) # share of imports for priv hhld at agent's prices #;
Formula (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    PMSHR(i,s,t) = VIPA(i,s,t) / VPA(i,s,t);

Equation PCOMPRICE
# private consumption price for composite commodities (HT 47) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    pp(i,s,t) = PMSHR(i,s,t) * ppm(i,s,t) + [1 - PMSHR(i,s,t)] * ppd(i,s,t);

Equation PHHLDDOM
# private consumption demand for domestic goods (HT 48) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    qpd(i,s,t) = qp(i,s,t) + ESUBD(i,t) * [pp(i,s,t) - ppd(i,s,t)];

Equation PHHLDAGRIMP
# private consumption demand for aggregate imports (HT 49) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    qpm(i,s,t) = qp(i,s,t) + ESUBD(i,t) * [pp(i,s,t) - ppm(i,s,t)];


!<
    ---------
    3. Firms
    ---------

    We now turn to the behavioral equations for firms.  The following picture
    describes factor demands.  The first set of equations describe demands for
    primary factors.  (See table 4 of Hertel and Tsigas.)

    Production structure
    --------------------

                             qo(j,r,t)  [ao(j,r,t)]
                             /\
                            /  \  <----- CES
                           /    \
                          /      \
                         /        \
      [ava(j,r,t)]  qva(j,r,t)        qf(i,j,r,t)  [af(i,j,r,t)]
                  /|\                    /\
 CES ----->      / | \                  /  \  <------ CES
                /  |  \                /    \
               /   |   \              /      \
              /    |    \            /        \
          Land   Labor  Capital  Domestic     Foreign
               qfe(i,j,r,t)         qfd(i,j,r,t)  qfm(i,j,r,t)
             [afe(i,j,r,t)]



    3-0. Module-Specific Variables
    3-1. Total Output Nest
    3-2. Composite Intermediates Nest
    3-3. Value-Added Nest
    3-4. Zero Profits Equations
>!


!<
    3-0. Module-Specific Variables
    ------------------------------

    only used in this Firms module or the Summary Indices or Welfare
    appendices
>!

Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pva(j,r,t) # firms' price of value added in industry j of region r #;
Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qva(j,r,t) # value added in industry j of region r #;
Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pf(i,j,r,t) # firms' price for commodity i for use by j in r #;
Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qf(i,j,r,t) # demand for commodity i for use by j in region r #;
Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ao(j,r,t) # output augmenting technical change in sector j of r #;
Variable (all,i,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ava(i,r,t) # value added augmenting tech change in sector i of r #;
Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    af(i,j,r,t) #composite intermed. input i augmenting tech change by j of r #;
Variable (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    afe(i,j,r,t) # primary factor i augmenting tech change by j of r #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    ams(i,r,s,t) # import i from region r augmenting tech change in region s #;


!<
    3-1. Total Output Nest
    ----------------------
>!

Variable (all,j,PROD_COMM)(all,t,ALLTIME)
    aosec(j,t) # output tech change of sector j, worldwide #;
Variable (all,r,REG)(all,t,ALLTIME)
    aoreg(r,t) # output tech change in region r #;
Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    aoall(j,r,t) # output augmenting technical change in sector j of r #;
Equation AOWORLD
# sector/region specific average rate of output augmenting tech change #
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ao(j,r,t) = aosec(j,t) + aoreg(r,t) + aoall(j,r,t);

Variable (all,j,PROD_COMM)(all,t,ALLTIME)
    avasec(j,t) # value added tech change of sector j, worldwide #;
Variable (all,r,REG)(all,t,ALLTIME)
    avareg(r,t) # value added tech change in region r #;
Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    avaall(j,r,t) # value added augmenting technical change in sector j of r #;
Equation AVAWORLD
# sector/region specific average rate of value added augmenting tech change #
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ava(j,r,t) = avasec(j,t) + avareg(r,t) + avaall(j,r,t);

Coefficient (parameter)(all,j,PROD_COMM)(all,t,ALLTIME)
    ESUBT(j,t) # elst. of sub. among composite intermediate inputs in production #;
Read
    ESUBT from file GTAPPARM header "ESBT";

Equation VADEMAND
# sector demands for primary factor composite #
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qva(j,r,t)
        = -ava(j,r,t) + qo(j,r,t) - ao(j,r,t)
        - ESUBT(j,t) * [pva(j,r,t) - ava(j,r,t) - ps(j,r,t) - ao(j,r,t)];
!<
    Sector demands for primary factor composite.  This equation differs from
    HT 35 due to the presence of intermediate input substitution.
>!

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    afcom(i,t) # intermediate tech change of input i, worldwide #;
Variable (all,j,PROD_COMM)(all,t,ALLTIME)
    afsec(j,t) # intermediate tech change of sector j, worldwide #;
Variable (all,r,REG)(all,t,ALLTIME)
    afreg(r,t) # intermediate tech change in region r #;
Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    afall(i,j,r,t) # intermediate input i augmenting tech change by j in r #;
Equation AFWORLD
# sector/region specific average rate of intermediates augmenting tech change #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    af(i,j,r,t) = afcom(i,t) + afsec(j,t) + afreg(r,t) + afall(i,j,r,t);

Equation INTDEMAND
# industry demands for intermediate inputs, including cgds #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qf(i,j,r,t)
        = - af(i,j,r,t) + qo(j,r,t) - ao(j,r,t)
        - ESUBT(j,t) * [pf(i,j,r,t) - af(i,j,r,t) - ps(j,r,t) - ao(j,r,t)];
!<
    Industry demands for intermediate inputs, including cgds.  This equation
    differs from HT 36 due to the presence of intermediate input substitution.
>!


!<
    3-2. Composite Intermediates Nest
    ---------------------------------
>!

Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    tfd(i,j,r,t) # tax on domestic i purchased by j in r #;
Equation DMNDDPRICE
# eq'n links domestic market and firm prices (HT 20) #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pfd(i,j,r,t) = tfd(i,j,r,t) + pm(i,r,t);
!<
    This equation links domestic market and firm prices.  It holds only for
    domestic goods and it captures the effect of commodity taxation of firms.
    (HT 20)
>!

Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    tfm(i,j,r,t) # tax on imported i purchased by j in r #;
Equation DMNDIPRICES
# eq'n links domestic market and firm prices (HT 23) #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pfm(i,j,r,t) = tfm(i,j,r,t) + pim(i,r,t);
!<
    This equation links domestic market and firm prices.  It holds only for
    imported goods and it captures the effect of commodity taxation of firms.
    (HT 23)
>!

Equation TIURATIO
# change in ratio of tax payments on intermediate goods to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxriu(r,t) + TIU(r,t) * y(r,t)
        = sum(i,TRAD_COMM, sum(j,PROD_COMM,
            VDFA(i,j,r,t) * tfd(i,j,r,t)
             + DFTAX(i,j,r,t) * [pm(i,r,t) + qfd(i,j,r,t)]))
        + sum(i,TRAD_COMM, sum(j,PROD_COMM,VIFA(i,j,r,t) * tfm(i,j,r,t)
             + IFTAX(i,j,r,t) * [pim(i,r,t) + qfm(i,j,r,t)]));

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)(all,t,ALLTIME)
    FMSHR(i,j,s,t) # share of firms' imports in dom. composite, agent's prices#;
Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)(all,t,ALLTIME)
    FMSHR(i,j,s,t) = VIFA(i,j,s,t) / VFA(i,j,s,t);
Zerodivide (zero_by_zero) off;
Equation ICOMPRICE
# industry price for composite commodities (HT 30) #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pf(i,j,r,t) = FMSHR(i,j,r,t) * pfm(i,j,r,t)
                   + [1 - FMSHR(i,j,r,t)] * pfd(i,j,r,t);

Equation INDIMP
# industry j demands for composite import i (HT 31) #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)(all,t,ALLTIME)
    qfm(i,j,s,t) = qf(i,j,s,t) - ESUBD(i,t) * [pfm(i,j,s,t) - pf(i,j,s,t)];

Equation INDDOM
# industry j demands for domestic good i (HT 32) #
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)(all,t,ALLTIME)
    qfd(i,j,s,t) = qf(i,j,s,t) - ESUBD(i,t) * [pfd(i,j,s,t) - pf(i,j,s,t)];


!<
    3-3. Value-Added Nest
    ---------------------
>!

Variable (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    tf(i,j,r,t) # tax on primary factor i used by j in region r #;

Equation MPFACTPRICE
# eq'n links domestic and firm demand prices (HT 16) #
(all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pfe(i,j,r,t) = tf(i,j,r,t) + pm(i,r,t);

Equation SPFACTPRICE
# eq'n links domestic and firm demand prices (HT 17) #
(all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pfe(i,j,r,t) = tf(i,j,r,t) + pmes(i,j,r,t);

Variable (all,i,ENDW_COMM)(all,t,ALLTIME)
    afecom(i,t) # factor input tech change of input i, worldwide #;
Variable (all,j,PROD_COMM)(all,t,ALLTIME)
    afesec(j,t) # factor input tech change of sector j, worldwide #;
Variable (all,r,REG)(all,t,ALLTIME)
    afereg(r,t) # factor input tech change in region r #;
Variable (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    afeall(i,j,r,t) # primary factor i augmenting tech change sector j in r #;
Equation AFEWORLD
# sector/region specific average rate of prim. factor i augmenting tech change #
(all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    afe(i,j,r,t) = afecom(i,t) + afesec(j,t) + afereg(r,t) + afeall(i,j,r,t);

Coefficient (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    VVA(j,r,t) # value added in activity j in region r #;
Formula (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    VVA(j,r,t) = sum(i,ENDW_COMM, VFA(i,j,r,t));
Coefficient (all,i,ENDW_COMM)(all,t,ALLTIME)
    SVADEFAULT(i,t) #zerodivide default for SVA#;
Formula (all,i,ENDW_COMM)(all,t,ALLTIME)
    SVADEFAULT(i,t)
        = sum(j,PROD_COMM, sum(r,REG, VFA(i,j,r,t)))
        / sum(j,PROD_COMM, sum(r,REG, VVA(j,r,t)));
Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SVA(i,j,r,t) # share of i in total value added in j in r #;
Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,t,ALLTIME)
(all,r,REG: VVA(j,r,t) <> 0)
    SVA(i,j,r,t) = VFA(i,j,r,t) / VVA(j,r,t);
Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,t,ALLTIME)
(all,r,REG: VVA(j,r,t) = 0)
    SVA(i,j,r,t) = SVADEFAULT(i,t);
Equation VAPRICE
# effective price of primary factor composite in each sector/region (HT 33) #
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    pva(j,r,t) = sum(k,ENDW_COMM, SVA(k,j,r,t) * [pfe(k,j,r,t) - afe(k,j,r,t)]);

Equation TFURATIO
# change in ratio of tax payments on factor usage to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrfu(r,t) + TFU(r,t) * y(r,t)
        = sum(i,ENDWM_COMM, sum(j,PROD_COMM,VFA(i,j,r,t) * tf(i,j,r,t)
               + ETAX(i,j,r,t) * [pm(i,r,t) + qfe(i,j,r,t)]))
        + sum(i,ENDWS_COMM, sum(j,PROD_COMM,VFA(i,j,r,t) * tf(i,j,r,t)
               + ETAX(i,j,r,t) * [pmes(i,j,r,t) + qfe(i,j,r,t)]));

Coefficient (parameter)(all,j,PROD_COMM)(all,t,ALLTIME)
    ESUBVA(j,t) # elst. of sub. capital/labor/land, in production of value added in j #;
Read
    ESUBVA from file GTAPPARM header "ESBV";

Equation ENDWDEMAND
# demands for endowment commodities (HT 34) #
(all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qfe(i,j,r,t)
        = - afe(i,j,r,t) + qva(j,r,t)
        - ESUBVA(j,t) * [pfe(i,j,r,t) - afe(i,j,r,t) - pva(j,r,t)];


!<
    3-4. Zero Profits Equations
    ---------------------------
>!

Equation OUTPUTPRICES
# eq'n links pre- and post-tax supply prices for all industries  (HT 15) #
(all,i,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ps(i,r,t) = to(i,r,t) + pm(i,r,t);
!<
    This equation links pre- and post-tax supply prices for all industries.
    This captures the effect of output taxes.  TO(i,r,t) < 1 in the case of a
    tax.  (HT 15)
>!

Equation TOUTRATIO
# change in ratio of output tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrout(r,t) + TOUT(r,t) * y(r,t)
        = sum(i,PROD_COMM,
            VOA(i,r,t) * [-to(i,r,t)] + PTAX(i,r,t) * [pm(i,r,t) + qo(i,r,t)]);

Variable (all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    profitslack(j,r,t) # slack variable in the zero profit equation #;
!<
    This is exogenous, unless the user wishes to specify output in a given
    region exogenously.
>!
Coefficient (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    STC(i,j,r,t) # share of i in total costs of j in r #;
Formula (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    STC(i,j,r,t) = VFA(i,j,r,t) / sum(k,DEMD_COMM, VFA(k,j,r,t));

Equation ZEROPROFITS
# industry zero pure profits condition (HT 6) #
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    ps(j,r,t) + ao(j,r,t)
        = sum(i,ENDW_COMM,
                STC(i,j,r,t) * [pfe(i,j,r,t) - afe(i,j,r,t) - ava(j,r,t)])
        + sum(i,TRAD_COMM, STC(i,j,r,t) * [pf(i,j,r,t)  - af(i,j,r,t)])
        + profitslack(j,r,t);
!<
    Industry zero pure profits condition (HT 6).  This condition permits us to
    determine the endogenous output level for each of the non-endowment
    sectors, excepting when profitslack is itself endogenous.  The level of
    activity in the endowment sectors is exogenously determined.
>!


!<
    ---------------------------------------
    4. Investment, Global Bank, and Savings
    ---------------------------------------

    Capital stock and rate of return equations follow.  They correspond to the
    Investment Equations of Table 8 in Hertel and Tsigas.

    4-0. Module-Specific Variables
    4-1. Equations of Notational Convenience
    4-2. Rate of Return Equations
    4-3. Capital Accumulation
    4-4. Global Bank
    4-5. Price Index of Aggregate Global Composite Capital Goods
    4-6. Price of Saving
>!


!<
    4-0. Module-Specific Variables
    ------------------------------

    only used in this Investment, Global Bank and Savings module
>!

!Variable (all,r,REG)(all,t,ALLTIME)
    rental(r,t) # rental rate on capital = ps("capital",r,t) #;!
!Variable (all,r,REG)(all,t,ALLTIME)
    ke(r,t) # end-of-period capital stock in r #;!
!Variable (all,r,REG)(all,t,ALLTIME)
    rore(r,t) # expected net rate of return on capital stock in r #;!
!Variable (all,r,REG)(all,t,ALLTIME)
    rorc(r,t) # current net rate of return on capital stock in r #;!
Variable (all,r,REG)(all,t,ALLTIME)
    qcgds(r,t) # output of capital goods sector = qo("zcgds",r,t) #;


!<
    4-1. Equations of Notational Convenience
    ----------------------------------------
>!

!Variable (all,r,REG)(all,t,ALLTIME)
    ksvces(r,t) # capital services = qo("capital",r,t) #;
Equation KAPSVCES
# eq'n defines a variable for capital services (HT 52) #
(all,r,REG)(all,t,ALLTIME)
    ksvces(r,t)
        = sum(h,ENDWC_COMM, [VOA(h,r,t)
            / sum(k,ENDWC_COMM, VOA(k,r,t))] * qo(h,r,t));!
!<
    This equation defines a variable for capital services, for convenience.
    (There is really only one capital services item.)(HT 52)
>!

!Equation KAPRENTAL
# eq'n defines a variable for capital rental rate (HT 53) #
(all,r,REG)(all,t,ALLTIME)
    rental(r,t)
        = sum(h,ENDWC_COMM, [VOA(h,r,t)
                     / sum(k,ENDWC_COMM, VOA(k,r,t))] * ps(h,r,t));!

Equation CAPGOODS
# eq'n defines a variable for gross investment (HT 54) #
(all,r,REG)(all,t,ALLTIME)
    qcgds(r,t)
        = sum(h,CGDS_COMM, [VOA(h,r,t) / REGINV(r,t)] * qo(h,r,t));
!<
    This equation defines a variable for gross investment, for convenience.
    There is really only one capital goods item.
>!

Equation PRCGOODS
# eq'n defines the price of cgds (HT 55) #
(all,r,REG)(all,t,ALLTIME)
    pcgds(r,t) = sum(h,CGDS_COMM, [VOA(h,r,t) / REGINV(r,t)] * ps(h,r,t));

!Equation KBEGINNING
# associates change in cap. services w/ change in cap. stock (HT 56) #
(all,r,REG)(all,t,ALLTIME)
    kb(r,t) = ksvces(r,t);!
!<
    This equation associates any change in capital services during the period
    with a change in capital stock.  Full capacity utilization is assumed.
>!

Coefficient (ge 0)(all,r,REG)(all,t,ALLTIME)
    VKB(r,t) # value of beginning-of-period capital stock in region r #;
Read
    VKB from file GTAPDATA header "VKB";
!<qo("capital",r,t)
  VKB(r,t) is used only in this sub-module, so its data are read here.  The
  associated variables, pcgds(r,t) and kb(r,t), are used across modules, so they
  have been defined as common variables.
>!

!Coefficient (all,r,REG)(all,t,ALLTIME)
    INVKERATIO(r,t) # ratio of gross investment to end-of-period capital stock in r #;
Formula (all,r,REG)(all,t,ALLTIME)
    INVKERATIO(r,t) = REGINV(r,t) / [VKB(r,t) + NETINV(r,t)];!

!Equation KEND
# Ending capital stock equals beginning stock plus net investment.  (HT 10) #
(all,r,REG)(all,t,ALLTIME)
    ke(r,t) = INVKERATIO(r,t) * qcgds(r,t) + [1.0 - INVKERATIO(r,t)] * kb(r,t);!


!<
    4-2. Rate of Return Equations
    -----------------------------
>!

!Coefficient (all,r,REG)(all,t,ALLTIME)
    GRNETRATIO(r,t) # ratio of GROSS/NET rates of return on capital in r #;
Formula (all,r,REG)(all,t,ALLTIME)
    GRNETRATIO(r,t)
        = sum(h,ENDWC_COMM, VOA(h,r,t))
        / [sum(h,ENDWC_COMM, VOA(h,r,t)) - VDEP(r,t)];!
!<
    NOTE: VOA("capital",r,t) is GROSS returns to capital.
>!
!Equation RORCURRENT
# current rate of return on capital in region r (HT 57) #
(all,r,REG)(all,t,ALLTIME)
    rorc(r,t) = GRNETRATIO(r,t) * [rental(r,t) - pcgds(r,t)];!

!Coefficient (parameter)(all,r,REG)(all,t,ALLTIME)
    RORFLEX(r,t) # flexibility of expected net ROR on capital stock in r wrt investment #;
Read
    RORFLEX from file GTAPPARM header "RFLX";!
!<
    RORFLEX is the flexibility of the expected net rate of return on the
    capital stock, in region r, with respect to investment.  If a region's
    capital stock increases by 1%, then it is expected that the net rate of
    return on capital will decline by RORFLEX %.  A larger absolute value for
    this parameter will reduce the tendency of international investment flows
    to respond to changes in expected rates of return.
>!

!Equation ROREXPECTED
# expected rate of return depends on the current return and investment (HT 58) #
(all,r,REG)(all,t,ALLTIME)
    rore(r,t) = rorc(r,t) - RORFLEX(r,t) * [ke(r,t) - kb(r,t)];!


!<
    4-3. Capital Accumulation
    -------------------------

    based on the theory laid out in GTAP Technical Paper #7
>!

!Variable (all,i,ENDWC_COMM)(all,r,REG)(all,t,ALLTIME)
    EXPAND(i,r,t) # change in investment levels relative to endowment stock #;
Equation BALDWIN
# change in investment levels relative to endowment stock #
(all,i,ENDWC_COMM)(all,r,REG)(all,t,ALLTIME)
    EXPAND(i,r,t) = qcgds(r,t) - qo(i,r,t);!


!<
    4-4. Global Bank
    ----------------
>!

!Coefficient (integer,parameter)(all,t,ALLTIME)
    RORDELTA(t) # binary coefficient to switch mechanism of allocating investment funds #;
Read
    RORDELTA from file GTAPPARM header "RDLT";!
!<
    RORDELTA is a binary coefficient which determines the mechanism of
    allocating investment funds across regions.  When RORDELTA = 1, investment
    funds are allocated across regions to equate the change in the expected
    rates of return (i.e., rore(r,t)).  When RORDELTA = 0, investment funds are
    allocated across regions to maintain the existing composition of capital
    stocks.
>!

Variable (all,t,ALLTIME)
    rorg(t) # global net rate of return on capital stock #;
!Variable (all,r,REG)(all,t,ALLTIME)
    cgdslack(r,t) # slack variable for qcgds(r,t) #;!
!<
    This is exogenous, unless the user wishes to exogenously fix the level of
    gross investment in a region, in which case it can be swapped with
    cgdslack.
>!
!Equation RORGLOBAL
# either gross investment or expected rate of return in region r (HT 59) #
(all,r,REG)(all,t,ALLTIME)
    RORDELTA(t) * rore(r,t)
    + [1 - RORDELTA(t)]
    * [[REGINV(r,t) / NETINV(r,t)] * qcgds(r,t)
          - [VDEP(r,t) / NETINV(r,t)] * qo("capital",r,t)]
        = RORDELTA(t) * rorg(t) + [1 - RORDELTA(t)] * globalcgds(t)
          + cgdslack(r,t);!
!<qo("capital",r,t)
    This equation determines either gross investment or the expected rate of
    return in each region, depending on the setting for the binary RORDELTA
    parameter.
>!

!Equation GLOBALINV
# either expected global rate of return or global net investment (HT 11) #
(all,t,ALLTIME)
    RORDELTA(t) * globalcgds(t) + [1 - RORDELTA(t)] * rorg(t)
        = RORDELTA(t)
        * sum(r,REG,
            [REGINV(r,t) / GLOBINV(t)] * qcgds(r,t)
               - [VDEP(r,t) / GLOBINV(t)] * qo("capital",r,t))
        + [1-RORDELTA(t)] * sum(r,REG, [NETINV(r,t) / GLOBINV(t)] * rore(r,t));!
!<qo("capital",r,t)
    This equation computes either the change in global net investment (when
    RORDELTA = 1), or the change in the expected global rate of return on
    capital (when RORDELTA = 0).
>!

!Equation RORGLOBAL
# either gross investment or expected rate of return in region r (HT 59) #
(all,r,REG)(all,t,ALLTIME)
    [REGINV(r,t) / NETINV(r,t)] * qcgds(r,t)
        - [VDEP(r,t) / NETINV(r,t)] * qo("capital",r,t)
        = globalcgds(t);!
! + cgdslack(r,t);!

!Equation GLOBALINV
# either expected global rate of return or global net investment (HT 11) #
(all,t,ALLTIME)
    rorg(t)
        = sum(r,REG, [NETINV(r,t) / GLOBINV(t)] * rore(r,t));!

Equation GLOBALINV
# either expected global rate of return or global net investment (HT 11) #
(all,t,ALLTIME)
    globalcgds(t)
        = sum(r,REG,
            [REGINV(r,t) / GLOBINV(t)] * qcgds(r,t)
              - [VDEP(r,t) / GLOBINV(t)] * qo("capital",r,t));

!<
    4-5. Price Index of Aggregate Global Composite Capital Goods
    ------------------------------------------------------------
>!

Equation PRICGDS
# eq'n generates a price index for the aggregate global cgds composite (HT 60) #
(all,t,ALLTIME)
    pcgdswld(t) = sum(r,REG, [NETINV(r,t) / GLOBINV(t)] * pcgds(r,t));


!<
    4-6. Price of Saving
    --------------------
>!

Variable (all,r,REG)(all,t,ALLTIME)
    psaveslack(r,t) # slack variable for the savings price equation #;
!<
    This is exogenous under the normal closure.  However, in order to
    replicate simulations from the GTAP book, where there was a single savings
    price, psaveslack may be swapped with psave in order to fix the latter.
    In this case, pcgdswld must be the numeraire so that each region faces the
    same price of savings.
>!

Equation SAVEPRICE
# savings price #
(all,r,REG)(all,t,ALLTIME)
    psave(r,t)
        = pcgds(r,t)
        + sum(s,REG, [[NETINV(s,t) - SAVE(s,t)] / GLOBINV(t)] * pcgds(s,t))
        + psaveslack(r,t);
!<
    In contrast to the GTAP book, the price of savings is now region-specific
    and it changes at the same rate as the price of regional investment, plus
    an adjustment factor which accounts for the fact that savings and
    investment are not equal at the regional level.
>!


!<
    ----------------------
    5. International Trade
    ----------------------

    5-1. Export Prices
    5-2. Demand for Imports
>!


!<
    5-1. Export Prices
    ------------------
>!

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tx(i,r,t) # dest.-gen. change in subsidy on exports of i from r #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    txs(i,r,s,t) # dest.-spec. change in subsidy on exports of i from r to s #;
!<
    The variable txs captures changes in the power of bilateral export taxes.
    However, the presence of a destination-generic export subsidy shift (tx)
    also permits the user to swap a single export tax shock with another
    target variable.  It is most naturally swapped with the variable qo to
    insulate domestic producers from the world market.
>!

Equation EXPRICES
# eq'n links agent's and world prices (HT 27) #
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    pfob(i,r,s,t) = pm(i,r,t) - tx(i,r,t) - txs(i,r,s,t);

Equation TEXPRATIO
# change in ratio of export tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrexp(r,t) + TEX(r,t) * y(r,t)
        = sum(i,TRAD_COMM, sum(s,REG,
            VXMD(i,r,s,t) * [-tx(i,r,t) - txs(i,r,s,t)]
            + XTAXD(i,r,s,t) * [pfob(i,r,s,t) + qxs(i,r,s,t)]));


!<
    5-2. Demand for Imports
    -----------------------

    Composite Imports Nest: Table 3 of Hertel and Tsigas
>!

Variable (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    tm(i,s,t) # source-gen. change in tax on imports of i into s #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    tms(i,r,s,t) # source-spec. change in tax on imports of i from r into s #;
!<
    The variable tms captures changes in the power of bilateral import taxes.
    However, the presence of a source-generic import tariff shift (tm) also
    permits the user to swap a single import tariff shock with another target
    variable.  In particular, to insulate domestic producers from import price
    changes, it may be swapped with the relative price variable pr -- see
    below.
>!

Equation MKTPRICES
# eq'n links domestic and world prices (HT 24) #
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    pms(i,r,s,t) = tm(i,s,t) + tms(i,r,s,t) + pcif(i,r,s,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    MSHRS(i,r,s,t) # share of imports from r in import bill of s at mkt prices#;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    MSHRS(i,r,s,t) = VIMS(i,r,s,t) / sum(k,REG, VIMS(i,k,s,t));
Equation DPRICEIMP
# price for aggregate imports (HT 28) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    pim(i,s,t) = sum(k,REG, MSHRS(i,k,s,t) * [pms(i,k,s,t) - ams(i,k,s,t)]);

Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pr(i,r,t) # ratio of domestic to imported prices in r #;
Equation PRICETGT
# eq'n defines target price ratio to be attained via the variable levy (HT 25) #
(all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    pr(i,s,t) = pm(i,s,t) - pim(i,s,t);
!<
    This equation defines the target price ratio to be attained via the
    variable levy.  This price ratio is the ratio of domestic to average
    imported goods' price.  Note that the way this price ratio is defined, it
    includes intraregional imports as well.  In most applications, regions
    will represent groups of individual countries.  However, in the case of
    the EU, this is problematic, since recent versions of the database have
    incorporated intra-EU trade flows.  Therefore, when aggregated to the EU
    level, the composite import price includes both intra-EU and outside
    imports.  So some modification is needed to handle the EU case.
>!

Coefficient (parameter)(all,i,TRAD_COMM)(all,t,ALLTIME)
    ESUBM(i,t) # region-generic el. of sub. among imports of i in Armington structure #;
Read
    ESUBM from file GTAPPARM header "ESBM";

Equation IMPORTDEMAND
# regional demand for disaggregated imported commodities by source (HT 29)  #
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    qxs(i,r,s,t)
        = -ams(i,r,s,t) + qim(i,s,t)
        - ESUBM(i,t) * [pms(i,r,s,t) - ams(i,r,s,t) - pim(i,s,t)];

Equation TIMPRATIO
# change in ratio of import tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrimp(r,t) + TIM(r,t) * y(r,t)
        = sum(i,TRAD_COMM, sum(s,REG,
            VIMS(i,s,r,t) * [tm(i,r,t) + tms(i,s,r,t)]
            + MTAX(i,s,r,t) * [pcif(i,s,r,t) + qxs(i,s,r,t)]));


!<
    -----------------------------------
    6. International Transport Services
    -----------------------------------

    6-0. Module-Specific Variables and Coefficients
    6-1. Demand for Global Transport Services
    6-2. Supply of Transport Services
>!


!<
    6-0. Module-Specific Variables and Coefficients
    -----------------------------------------------

    only used in this International Transport Services module
>!

Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    qtmfsd(m,i,r,s,t) # international usage margin m on i from r to s #;
!<
    International margin usage, by Margin, Freight, Source, and Destination,
    i.e., the percent change in usage of m in transport of i from r to s.
>!
Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    atmfsd(m,i,r,s,t) # tech change in m's shipping of i from region r to s #;
!<
    Technical progress in shipping by Margin, Freight, Source, and
    Destination.  This is endogenous and driven by the following mode-,
    product-, source-, and destination-specific determinants.
>!
Variable (all,m,TRAD_COMM)(all,t,ALLTIME)
    atm(m,t) # tech change in mode m, worldwide #;
Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    atf(i,t) # tech change shipping of i, worldwide #;
Variable (all,r,REG)(all,t,ALLTIME)
    ats(r,t) # tech change shipping from region r #;
Variable (all,s,REG)(all,t,ALLTIME)
    atd(s,t) # tech change shipping to s #;
Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    atall(m,i,r,s,t) # tech change in m's shipping of i from region r to s #;
Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    ptrans(i,r,s,t) # cost index for international transport of i from r to s #;
!<
    average cost index for margin services used in getting i from r to s
>!
Variable (all,m,MARG_COMM)(all,t,ALLTIME)
    qtm(m,t) # global margin usage #;
Variable (all,m,MARG_COMM)(all,t,ALLTIME)
    pt(m,t) # price of composite margins services, type #;
!<
    price index for commodity m in margin services usage
>!

Coefficient (ge 0)(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTMFSD(m,i,r,s,t) # int'l margin usage, by margin, freight, source, and destination #;
Update (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTMFSD(m,i,r,s,t) = pt(m,t) * qtmfsd(m,i,r,s,t);
Read
    VTMFSD from file GTAPDATA header "VTWR";

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTFSD(i,r,s,t) # aggregate value of svces in the shipment of i from r to s#;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTFSD(i,r,s,t) = sum(m,MARG_COMM, VTMFSD(m,i,r,s,t));
!<
    In a balanced data base, (all,i,TRAD_COMM)(all,r,REG)(all,s,REG),
    VIWS(i,r,s,t) = VXWD(i,r,s,t) + VTFSD(i,r,s,t).
>!

Coefficient (all,m,MARG_COMM)(all,t,ALLTIME)
    VTMUSE(m,t) # international margin services usage, by type #;
Formula (all,m,MARG_COMM)(all,t,ALLTIME)
    VTMUSE(m,t) = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG, VTMFSD(m,i,r,s,t))));

Coefficient (all,m,MARG_COMM)(all,t,ALLTIME)
    VTMPROV(m,t) # international margin services provision #;
Formula (all,m,MARG_COMM)(all,t,ALLTIME)
    VTMPROV(m,t) = sum(r,REG, VST(m,r,t));
!<
    In a balanced data base, VTMPROV = VTMUSE.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    VTRPROV(r,t) # international margin supply, by region #;
Formula (all,r,REG)(all,t,ALLTIME)
    VTRPROV(r,t) = sum(m,MARG_COMM, VST(m,r,t));

Coefficient (all,t,ALLTIME)
    VT(t) # international margin supply #;
Formula (all,t,ALLTIME)
    VT(t) = sum(m,MARG_COMM, sum(r,REG, VST(m,r,t)));


!<
    6-1. Demand for Global Transport Services
    -----------------------------------------
>!

Equation QTRANS_MFSD
# bilateral demand for transport services  #
(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    qtmfsd(m,i,r,s,t) = qxs(i,r,s,t) - atmfsd(m,i,r,s,t);
!<
    This equation computes the bilateral demand for international
    transportation services.  It reflects the fact that the demand for
    services along any particular route is proportional to the quantity of
    merchandise shipped [i.e., QXS(i,r,s,t)].  It is here that we introduce the
    potential for input-augmenting tech change, atmfsd(m,i,r,s,t), which is
    commodity- and route-specific.  Thus, in the levels:
            ATMFSD(m,i,r,s,t) * QTMFSD(m,i,r,s,t) = QXS(i,r,s,t)
    where QTMFSD is the amount of composite margins services m used along this
    route.  Technological improvements are reflected by atmfsd(i,r,s,t) > 0, and
    these reduce the margins services required for this i,r,s triplet.  Tech.
    change also dampens the cost of shipping, thereby lowering the CIF price
    implied by a given FOB value (see 6-2).
>!

Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTMUSESHR(m,i,r,s,t) # share of i,r,s usage in global demand for m #;
Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTMUSESHR(m,i,r,s,t) = VTFSD(i,r,s,t) / VT(t);
Formula
    (all,t,ALLTIME)(all,m,MARG_COMM: VTMUSE(m,t) <> 0.0)(all,i,TRAD_COMM)
    (all,r,REG)(all,s,REG)
    VTMUSESHR(m,i,r,s,t) = VTMFSD(m,i,r,s,t) / VTMUSE(m,t);

Equation TRANS_DEMAND
# global demand for margin m #
(all,m,MARG_COMM)(all,t,ALLTIME)
    qtm(m,t)
        = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
          VTMUSESHR(m,i,r,s,t) * qtmfsd(m,i,r,s,t))));


!<
    6-2. Supply of Transport Services
    ---------------------------------
>!

Coefficient (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VTSUPPSHR(m,r,t) # share of region r in global supply of margin m #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    VTSUPPSHR(m,r,t) = VTRPROV(r,t) / VT(t);
Formula (all,t,ALLTIME)(all,m,MARG_COMM: VTMPROV(m,t) <> 0.0)(all,r,REG)
    VTSUPPSHR(m,r,t) = VST(m,r,t) / VTMPROV(m,t);

Equation PTRANSPORT
# generate price index for composite transportation services #
(all,m,MARG_COMM)(all,t,ALLTIME)
    pt(m,t) = sum(r,REG, VTSUPPSHR(m,r,t) * pm(m,r,t));
!<
    This equation generates a price index for transportation services based on
    zero profits.  NOTE:
    (1) Sales to international transportation are not subject to export tax.
        This is why we base the costs to the transport sector on market prices
        of the goods sold to international transportation.
    (2) We assume that the supply shares for margin services are uniform
        across freight, source of freight, and destination.
    (cf. HT#7)
>!

Coefficient (all,t,ALLTIME)
    VTUSE(t) # international margin services usage #;
Formula (all,t,ALLTIME)
    VTUSE(t)
        = sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
            VTMFSD(m,i,r,s,t)))));

Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VTFSD_MSH(m,i,r,s,t) # share of margin m in cost of getting i from r to s #;
Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
        (all,s,REG: VTFSD(i,r,s,t) > 0.0)
    VTFSD_MSH(m,i,r,s,t) = VTMFSD(m,i,r,s,t) / VTFSD(i,r,s,t);
Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
        (all,s,REG: VTFSD(i,r,s,t) = 0.0)
    VTFSD_MSH(m,i,r,s,t) = VTMUSE(m,t) / VTUSE(t);

Equation TRANSCOSTINDEX
#  generates flow-specific modal average cost of transport index (cf. HT7) #
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    ptrans(i,r,s,t)
        = sum(m,MARG_COMM,VTFSD_MSH(m,i,r,s,t) * [pt(m,t) - atmfsd(m,i,r,s,t)]);

Equation TRANSTECHANGE
#  generates flow-specific average rate of technical change #
(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    atmfsd(m,i,r,s,t) = atm(m,t) + atf(i,t) + ats(r,t)
                         + atd(s,t) + atall(m,i,r,s,t);

Equation TRANSVCES
# generate demand for regional supply of global transportation service (HT 61) #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    qst(m,r,t) = qtm(m,t) + [pt(m,t) - pm(m,r,t)];
!<
    This equation generates the international transport sector's derived
    demand for regional supplies of transportation services.  It reflects a
    unitary elasticity of substitution between transportation services inputs
    from different regions.
>!

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VIWSCOST(i,r,s,t) # value of imports calculated as total cost of imports #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    VIWSCOST(i,r,s,t) = VXWD(i,r,s,t) + VTFSD(i,r,s,t);

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    FOBSHR(i,r,s,t) # FOB share in VIW #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    FOBSHR(i,r,s,t) = VXWD(i,r,s,t) / VIWSCOST(i,r,s,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    TRNSHR(i,r,s,t) # transport share in VIW #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    TRNSHR(i,r,s,t) = VTFSD(i,r,s,t) / VIWSCOST(i,r,s,t);
Zerodivide (zero_by_zero) off;

Equation FOBCIF
# eq'n links FOB and CIF prices for good i shipped from region r to s (HT 26') #
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    pcif(i,r,s,t)
       = FOBSHR(i,r,s,t) * pfob(i,r,s,t)
       + TRNSHR(i,r,s,t) * ptrans(i,r,s,t);
!<
    This equation links export and import prices for each commodity/route
    triplet.  Note that technical change is embodied in ptrans(i,r,s,t) which is
    now a cost index, as opposed to (HT 26') where it represented the price of
    margins services.
>!


!<
    ---------------------
    7. Regional Household
    ---------------------

    7-0. Module-Specific Coefficients
    7-1. Supply of Endowments by the Regional Household
    7-2. Computation of Regional Income
    7-3. Regional Household Demand System
    7-4. Aggregate Utility
>!


!<
    7-0. Module-Specific Coefficients
    ---------------------------------

    only used in this Regional Household module
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRPRIV(r,t) # private expenditure share in regional income #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRPRIV(r,t) = PRIVEXP(r,t) / INCOME(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRGOV(r,t) # government expenditure share in regional income #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRGOV(r,t) = GOVEXP(r,t) / INCOME(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    XSHRSAVE(r,t) # saving share in regional income #;
Formula (all,r,REG)(all,t,ALLTIME)
    XSHRSAVE(r,t) = SAVE(r,t) / INCOME(r,t);

Variable (all,r,REG)(all,t,ALLTIME)
    uelas(r,t) # elasticity of cost of utility wrt utility #;
Variable (all,r,REG)(all,t,ALLTIME)
    dppriv(r,t) # private consumption distribution parameter #;
Variable (all,r,REG)(all,t,ALLTIME)
    dpgov(r,t) # government consumption distribution parameter #;
Variable (all,r,REG)(all,t,ALLTIME)
    dpsave(r,t) # saving distribution parameter #;


!<
    7-1. Supply of Endowments by the Regional Household
    ---------------------------------------------------
>!

Equation FACTORINCPRICES
# eq'n links pre- and post-tax endowment supply prices (HT 15) #
(all,i,ENDW_COMM)(all,r,REG)(all,t,ALLTIME)
    ps(i,r,t) = to(i,r,t) + pm(i,r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    TINC(r,t) # income tax payments in r #;
Formula (all,r,REG)(all,t,ALLTIME)
    TINC(r,t) = sum(i,ENDW_COMM, PTAX(i,r,t));

Equation TINCRATIO
# change in ratio of income tax payments to regional income #
(all,r,REG)(all,t,ALLTIME)
    100.0 * INCOME(r,t) * del_taxrinc(r,t) + TINC(r,t) * y(r,t)
        = sum(i,ENDW_COMM,
            VOA(i,r,t) * [-to(i,r,t)] + PTAX(i,r,t) * [pm(i,r,t) + qo(i,r,t)]);

Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    REVSHR(i,j,r,t) # share of endowment commodity i used by fiorms in sector j of regin r evaluated at market prices #;
Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    REVSHR(i,j,r,t) = VFM(i,j,r,t) / sum(k,PROD_COMM, VFM(i,k,r,t));
Equation ENDW_PRICE
# eq'n generates the composite price for sluggish endowments (HT 50) #
(all,i,ENDWS_COMM)(all,r,REG)(all,t,ALLTIME)
    pm(i,r,t) = sum(k,PROD_COMM, REVSHR(i,k,r,t) * pmes(i,k,r,t));

Coefficient (parameter)(all,i,ENDWS_COMM)(all,t,ALLTIME)
    ETRAE(i,t) # elst. of transformation for sluggish primary factor endowments #;
Read
    ETRAE from file GTAPPARM header "ETRE";
!<
    ETRAE is the elasticity of transformation for sluggish primary factor
    endowments.  It is non-positive, by definition.
>!
Equation ENDW_SUPPLY
# eq'n distributes the sluggish endowments across sectors (HT 51) #
(all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qoes(i,j,r,t) = qo(i,r,t) - endwslack(i,r,t)
                    + ETRAE(i,t) * [pm(i,r,t) - pmes(i,j,r,t)];


!<
    7-2. Computation of Regional Income
    -----------------------------------
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    FY(r,t) # primary factor income in r net of depreciation #;
Formula (all,r,REG)(all,t,ALLTIME)
    FY(r,t) = sum(i,ENDW_COMM, VOM(i,r,t)) - VDEP(r,t);
Variable (all,r,REG)(all,t,ALLTIME)
    fincome(r,t) # factor income at market prices net of depreciation #;
Equation FACTORINCOME
# factor income at market prices net of depreciation #
(all,r,REG)(all,t,ALLTIME)
    FY(r,t) * fincome(r,t)
        = sum(i,ENDW_COMM, VOM(i,r,t) * [pm(i,r,t) + qo(i,r,t)])
        - VDEP(r,t)  * [pcgds(r,t) + qo("capital",r,t)];

Variable (change)(all,r,REG)(all,t,ALLTIME)
    del_indtaxr(r,t) # change in ratio of indirect taxes to INCOME in r #;
Equation DINDTAXRATIO
# change in ratio of indirect taxes to INCOME in r #
(all,r,REG)(all,t,ALLTIME)
    del_indtaxr(r,t)
        = del_taxrpc(r,t) + del_taxrgc(r,t) + del_taxriu(r,t) + del_taxrfu(r,t)
        + del_taxrout(r,t) + del_taxrexp(r,t) + del_taxrimp(r,t);

Variable (change)(all,r,REG)(all,t,ALLTIME)
    del_ttaxr(r,t) # change in ratio of taxes to INCOME in r #;
Equation DTAXRATIO
# change in ratio of taxes to INCOME in r #
(all,r,REG)(all,t,ALLTIME)
    del_ttaxr(r,t)
        = del_taxrpc(r,t) + del_taxrgc(r,t) + del_taxriu(r,t) + del_taxrfu(r,t)
        + del_taxrout(r,t) + del_taxrexp(r,t) + del_taxrimp(r,t)
        + del_taxrinc(r,t);
!<
   This variable can be swapped with the commodity- and source-generic
   consumption tax shift, tp(r,t), in order to generate a tax replacement
   scenario, whereby taxes remain a constant share of national income.
>!

Variable (all,r,REG)(all,t,ALLTIME)
    incomeslack(r,t) # slack variable in the expression for regional income #;
!<
    This is exogenous, unless the user wishes to fix regional income.
>!

Coefficient (all,r,REG)(all,t,ALLTIME)
    INDTAX(r,t) # indirect tax receipts in r #;
Formula (all,r,REG)(all,t,ALLTIME)
    INDTAX(r,t) = TPC(r,t) + TGC(r,t) + TIU(r,t) + TFU(r,t) + TOUT(r,t)
                  + TEX(r,t) + TIM(r,t);

Equation REGIONALINCOME
# regional income = sum of primary factor income and indirect tax receipts #
(all,r,REG)(all,t,ALLTIME)
    INCOME(r,t) * y(r,t)
        = FY(r,t) * fincome(r,t)
        + 100.0 * INCOME(r,t) * del_indtaxr(r,t)
        + INDTAX(r,t) * y(r,t)
        + INCOME(r,t) * incomeslack(r,t);
!<
    This equation computes regional income as the sum of primary factor
    payment and indirect tax receipts.  The first term computes the change in
    endowment income, net of depreciation.  The subsequent terms compute the
    change in indirect tax receipts for various transactions taxes.
>!


!<
    7-3. Regional Household Demand System
    -------------------------------------
>!

Variable (all,r,REG)(all,t,ALLTIME)
    dpav(r,t) # average distribution parameter shift, for EV calc. #;
Equation DPARAV
# average distribution parameter shift #
(all,r,REG)(all,t,ALLTIME)
    dpav(r,t)
        = XSHRPRIV(r,t) * dppriv(r,t)
        + XSHRGOV(r,t) * dpgov(r,t)
        + XSHRSAVE(r,t) * dpsave(r,t);

Equation UTILITELASTIC
# elasticity of cost of utility wrt utility #
(all,r,REG)(all,t,ALLTIME)
    uelas(r,t) = XSHRPRIV(r,t) * uepriv(r,t) - dpav(r,t);

Equation PRIVCONSEXP
# private consumption expenditure #
(all,r,REG)(all,t,ALLTIME)
    yp(r,t) - y(r,t) = -[uepriv(r,t) - uelas(r,t)] + dppriv(r,t);

Equation GOVCONSEXP
# government consumption expenditure #
(all,r,REG)(all,t,ALLTIME)
    yg(r,t) - y(r,t) = uelas(r,t) + dpgov(r,t);

Equation SAVING
# saving #
(all,r,REG)(all,t,ALLTIME)
    psave(r,t) + qsave(r,t) - y(r,t) = uelas(r,t) + dpsave(r,t);


!<
    7-4. Aggregate Utility
    ----------------------
>!

Variable (all,r,REG)(all,t,ALLTIME)
    p(r,t) # price index for disposition of income by regional household #;
Equation PRICEINDEXREG
    # price index for disposition of income by regional household #
    (all,r,REG)(all,t,ALLTIME)
    p(r,t)
        = XSHRPRIV(r,t) * ppriv(r,t)
        + XSHRGOV(r,t) * pgov(r,t)
        + XSHRSAVE(r,t) * psave(r,t);

Variable (all,r,REG)(all,t,ALLTIME)
    au(r,t) # input-neutral shift in utility function #;
Variable (all,r,REG)(all,t,ALLTIME)
    dpsum(r,t) # sum of the distribution parameters #;

Coefficient (all,r,REG)(all,t,ALLTIME)
    DPARSUM(r,t) # sum of distribution parameters #;
Read
    DPARSUM from file GTAPDATA header "DPSM";
Update (all,r,REG)(all,t,ALLTIME)
    DPARSUM(r,t) = dpsum(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILELAS(r,t) # elasticity of cost of utility wrt utility #;
Formula (all,r,REG)(all,t,ALLTIME)
    UTILELAS(r,t)
        = [UELASPRIV(r,t) * XSHRPRIV(r,t) + XSHRGOV(r,t)
            + XSHRSAVE(r,t)] / DPARSUM(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    DPARPRIV(r,t) # private consumption distribution parameter #;
Formula (all,r,REG)(all,t,ALLTIME)
    DPARPRIV(r,t) = UELASPRIV(r,t) * XSHRPRIV(r,t) / UTILELAS(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    DPARGOV(r,t) # government consumption distribution parameter #;
Formula (all,r,REG)(all,t,ALLTIME)
    DPARGOV(r,t) = XSHRGOV(r,t) / UTILELAS(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    DPARSAVE(r,t) # saving distribution parameter #;
Formula (all,r,REG)(all,t,ALLTIME)
    DPARSAVE(r,t) = XSHRSAVE(r,t) / UTILELAS(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILPRIV(r,t) # utility from private consumption #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILPRIV(r,t) = 1.0;
Update (all,r,REG)(all,t,ALLTIME)
    UTILPRIV(r,t) = up(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILGOV(r,t) # utility from government consumption #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILGOV(r,t) = 1.0;
Update (all,r,REG)(all,t,ALLTIME)
    UTILGOV(r,t) = ug(r,t);

Coefficient (all,r,REG)(all,t,ALLTIME)
    UTILSAVE(r,t) # utility from saving #;
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    UTILSAVE(r,t) = 1.0;
Update (change)(all,r,REG)(all,t,ALLTIME)
    UTILSAVE(r,t) = [[qsave(r,t) - pop(r,t)] / 100] * UTILSAVE(r,t);

Variable (all,r,REG)(all,t,ALLTIME)
    u(r,t) # per capita utility from aggregate hhld expend. in region r #;
Equation UTILITY
# regional household utility #
(all,r,REG)(all,t,ALLTIME)
    u(r,t)
        = au(r,t)
        + DPARPRIV(r,t) * loge(UTILPRIV(r,t)) * dppriv(r,t)
        + DPARGOV(r,t) * loge(UTILGOV(r,t)) * dpgov(r,t)
        + DPARSAVE(r,t) * loge(UTILSAVE(r,t)) * dpsave(r,t)
        + [1.0 / UTILELAS(r,t)] * [y(r,t) - pop(r,t) - p(r,t)];

Equation DISTPARSUM
# sum of the distribution parameters #
(all,r,REG)(all,t,ALLTIME)
    DPARSUM(r,t) * dpsum(r,t)
        = DPARPRIV(r,t) * dppriv(r,t)
        + DPARGOV(r,t) * dpgov(r,t)
        + DPARSAVE(r,t) * dpsave(r,t);


!<
    -------------------------
    8. Equilibrium Conditions
    -------------------------

    8-1. Market Clearing Conditions
    8-2. Walras' Law
>!


!<
    8-1. Market Clearing Conditions
    -------------------------------
>!

Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDFM(i,j,r,t) #share of dom. prod. i used by sector j in r at mkt prices#;
Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDFM(i,j,r,t) = VDFM(i,j,r,t) / VDM(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDPM(i,r,t) # share of domestic prod. of i used by private hhlds in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDPM(i,r,t) = VDPM(i,r,t) / VDM(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDGM(i,r,t) # share of imports of i used by gov't hhlds in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDGM(i,r,t) = VDGM(i,r,t) / VDM(i,r,t);

Variable (orig_level=VDM)(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qds(i,r,t) # domestic sales of commodity i in r #;
Equation MKTCLDOM
# eq'n assures market clearing for domestic sales (HT 3) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qds(i,r,t)
        = sum(j,PROD_COMM, SHRDFM(i,j,r,t) * qfd(i,j,r,t))
        + SHRDPM(i,r,t) * qpd(i,r,t)
        + SHRDGM(i,r,t) * qgd(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDM(i,r,t) # share of domestic sales of i in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRDM(i,r,t) = VDM(i,r,t) / VOM(i,r,t);

Coefficient (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRST(m,r,t) # share of sales of m to global transport services in r #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRST(m,r,t) = VST(m,r,t) / VOM(m,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SHRXMD(i,r,s,t) # share of export sales of i to s in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SHRXMD(i,r,s,t) = VXMD(i,r,s,t) / VOM(i,r,t);

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    tradslack(i,r,t) # slack variable in tradeables market clearing condition #;
!<
    This is exogenous unless the user wishes to specify the price of
    tradeables exogenously, in which case the analysis becomes partial
    equilibrium and walraslack must be exogenized.
>!

Equation MKTCLTRD_MARG
# eq'n assures market clearing for margins commodities (HT 1) #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    qo(m,r,t)
        = SHRDM(m,r,t) * qds(m,r,t)
        + SHRST(m,r,t) * qst(m,r,t)
        + sum(s,REG, SHRXMD(m,r,s,t) * qxs(m,r,s,t))
        + tradslack(m,r,t);
Equation MKTCLTRD_NMRG
# eq'n assures market clearing for the non-margins commodities (HT 1) #
(all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    qo(i,r,t)
        = SHRDM(i,r,t) * qds(i,r,t)
        + sum(s,REG, SHRXMD(i,r,s,t) * qxs(i,r,s,t))
        + tradslack(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VIM(i,r,t) #value of imports of commodity i in r at domestic market prices#;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    VIM(i,r,t) = sum(j,PROD_COMM, VIFM(i,j,r,t)) + VIPM(i,r,t) + VIGM(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIFM(i,j,r,t) # share of import i used by sector j in r  #;
Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIFM(i,j,r,t) = VIFM(i,j,r,t) / VIM(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIPM(i,r,t) # share of import i used by private hhlds in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIPM(i,r,t) = VIPM(i,r,t) / VIM(i,r,t);

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIGM(i,r,t) # the share of import i used by gov't hhlds in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHRIGM(i,r,t) = VIGM(i,r,t) / VIM(i,r,t);

Equation MKTCLIMP
# eq'n assures mkt clearing for imported goods entering each region (HT 2) #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    qim(i,r,t)
        = sum(j,PROD_COMM, SHRIFM(i,j,r,t) * qfm(i,j,r,t))
        + SHRIPM(i,r,t) * qpm(i,r,t)
        + SHRIGM(i,r,t) * qgm(i,r,t);

Coefficient (all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHREM(i,j,r,t) #share of mobile endowment i used by sector j at mkt prices#;
Formula (all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    SHREM(i,j,r,t) = VFM(i,j,r,t) / VOM(i,r,t);

Equation MKTCLENDWM
# eq'n assures mkt clearing for perfectly mobile endowments in each r (HT 4) #
(all,i,ENDWM_COMM)(all,r,REG)(all,t,ALLTIME)
    qo(i,r,t) = sum(j,PROD_COMM, SHREM(i,j,r,t)*qfe(i,j,r,t))+endwslack(i,r,t);
!<
    This equation assures market clearing for perfectly mobile endowments (HT
    4)
>!

Equation MKTCLENDWS
# eq'n assures mkt clearing for imperfectly mobile endowments in each r (HT 5) #
(all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    qoes(i,j,r,t) = qfe(i,j,r,t);
!<
    This equation assures market clearing for sluggish endowments (HT 5)
>!


!<
    8-2. Walras' Law
    ----------------
>!

Variable (all,t,ALLTIME)
    walras_sup(t) # supply in omitted market--global supply of cgds composite #;
Equation WALRAS_S
# Extra eq'n computes change in supply in the omitted market. #
(all,t,ALLTIME)
    walras_sup(t) = pcgdswld(t) + globalcgds(t);
!<
    This is an extra equation which simply computes change in supply in the
    omitted market.  (modified from HT 12 to reflect the value as opposed to
    quantity change, in light of the new treatment of psave(r,t))
>!

Variable (all,t,ALLTIME)
    walras_dem(t) # demand in the omitted market--global demand for savings #;
Equation WALRAS_D
# Extra eq'n computes change in demand in the omitted market. #
(all,t,ALLTIME)
    GLOBINV(t) * walras_dem(t) =
           sum(r,REG, SAVE(r,t) * [psave(r,t) + qsave(r,t)]);
!<
    This is an extra equation which simply computes change in demand in the
    omitted market.  (modified from HT 13 to reflect the value as opposed to
    quantity change)
>!

Variable (all,t,ALLTIME)
    walraslack(t) # slack variable in the omitted market #;
!<
    This is endogenous under normal, GE closure.  If the GE links are broken,
    then this must be swapped with the numeraire, thereby forcing global
    savings to explicitly equal global investment.
>!
Equation WALRAS
# Check Walras' Law.  Value of "walraslack" should be zero. (HT 14) #
(all,t,ALLTIME)
    walras_sup(t) = walras_dem(t) + walraslack(t);
!<
    This equation checks Walras' Law.  The value of walraslack should be zero
    in any GE simulation.  (HT 14)
>!


