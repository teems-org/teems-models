        = [0.01 * EVSCALFACT(r,t)] * sum(j,PROD_COMM, VVA(j,r,t) * ava(j,r,t));

Variable (linear,change)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTtech_avajr(j,r,t) # cont. to EV of value added augmenting tech change in sector j #;
Equation CONT_EV_tech_avajr
(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTtech_avajr(j,r,t) = [0.01 * EVSCALFACT(r,t)] * VVA(j,r,t) * ava(j,r,t);

Variable (linear,change)(all,r,REG)(all,t,ALLTIME)
    CNTtech_afr(r,t) # cont. to regional EV of comp. int. input augmenting tech change #;
Equation CONT_EV_tech_afr
(all,r,REG)(all,t,ALLTIME)
    CNTtech_afr(r,t)
        = [0.01 * EVSCALFACT(r,t)]
        * sum(j,PROD_COMM, sum(i,TRAD_COMM,
            [VIFA(i,j,r,t) + VDFA(i,j,r,t)] * af(i,j,r,t)));

Variable (linear,change)(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTtech_afijr(i,j,r,t) # cont. to EV of composite i input augmenting tech change in sector j #;
Equation CONT_EV_tech_afijr
(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,t,ALLTIME)
    CNTtech_afijr(i,j,r,t)
        = [0.01*EVSCALFACT(r,t)]*[VIFA(i,j,r,t)+VDFA(i,j,r,t)]*af(i,j,r,t);

Variable (linear,change)(all,r,REG)(all,t,ALLTIME)
    CNTtech_atrr(r,t) # contribution to regional EV of technical change in transportation #;
Equation CONT_EV_tech_atrr
(all,r,REG)(all,t,ALLTIME)
    CNTtech_atrr(r,t)
        = [0.01 * EVSCALFACT(r,t)]
        * sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
            VTMFSD(m,i,s,r,t) * atmfsd(m,i,s,r,t))));

Variable (linear,change)(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    CNTtech_afmfdsd(m,i,r,s,t) # cont. to EV of tech change in transportation efficiency #;
Equation CONT_EV_tech_afmfdsd
(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    CNTtech_afmfdsd(m,i,r,s,t)
        = [0.01 * EVSCALFACT(s,t)] * VTMFSD(m,i,r,s,t) * atmfsd(m,i,r,s,t);

Variable (linear,change)(all,r,REG)(all,t,ALLTIME)
    CNTtech_amsr(r,t) #cont. to EV of bilateral import-augmenting tech change#;
Equation CONT_EV_tech_amsr
(all,r,REG)(all,t,ALLTIME)
    CNTtech_amsr(r,t)
        = [0.01 * EVSCALFACT(r,t)]
        * sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r,t) * ams(i,s,r,t)));

Variable (linear,change)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    CNTtech_amsirs(i,r,s,t) # cont. to EV of bilateral import augmenting tech change for TRAD_COMM i #;
Equation CONT_EV_tech_amsirs
(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    CNTtech_amsirs(i,r,s,t) = [0.01*EVSCALFACT(r,t)]*VIMS(i,r,s,t)*ams(i,r,s,t);

Variable (linear,change)(all,r,REG)(all,t,ALLTIME)
    CNTkbr(r,t) # cont. to EV of changes to beg. period capital stock and depreciation #;
Equation CONT_EV_kbr
(all,r,REG)(all,t,ALLTIME)
    CNTkbr(r,t)= -[0.01*EVSCALFACT(r,t)]*VDEP(r,t)*[qo("capital",r,t)-pop(r,t)];


!<
    -------------------------------
    D. Terms of Trade Decomposition
    -------------------------------

    Computations for decomposition the terms-of-trade effect
    Reference: Robert McDougall, SALTER No. 12 Working Paper
>!

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SX_IR(i,r,t) # share of good i in total exports from r #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SX_IR(m,r,t)
        = [sum(s,REG, VXWD(m,r,s,t)) + VST(m,r,t)]
        / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s,t)))
            + sum(l,MARG_COMM, VST(l,r,t))];
Formula (all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    SX_IR(i,r,t)
        = sum(s,REG, VXWD(i,r,s,t))
        / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s,t)))
            + sum(l,MARG_COMM, VST(l,r,t))];

Coefficient (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    VTICOMM(m,s,t) # margin usage of m in getting imports to region s #;
Formula (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    VTICOMM(m,s,t) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s,t)));

Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    VIWDIRALL(i,s,t) # imports of i into s, with direct allocation of margins #;
Formula (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    VIWDIRALL(m,s,t) = sum(r,REG, VXWD(m,r,s,t)) + VTICOMM(m,s,t);
Formula (all,i,NMRG_COMM)(all,s,REG)(all,t,ALLTIME)
    VIWDIRALL(i,s,t) = sum(r,REG, VXWD(i,r,s,t));

Coefficient (all,s,REG)(all,t,ALLTIME)
    VIWDATOT(s,t) #total imports into s, calculated on direct allocation basis#;
Formula (all,s,REG)(all,t,ALLTIME)
    VIWDATOT(s,t) = sum(i,TRAD_COMM, VIWDIRALL(i,s,t));

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SM_IR(i,r,t)  # share of good i in total imports into r #;
Formula (all,i,TRAD_COMM)(all,s,REG)(all,t,ALLTIME)
    SM_IR(i,s,t) = VIWDIRALL(i,s,t) / VIWDATOT(s,t);

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SX_IRS(i,r,s,t) # share of exports of good i from region r to s #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SX_IRS(m,r,s,t) = VXWD(m,r,s,t) / [sum(k,REG, VXWD(m,r,k,t)) + VST(m,r,t)];
Formula (all,i,NMRG_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SX_IRS(i,r,s,t) = VXWD(i,r,s,t) / sum(k,REG, VXWD(i,r,k,t));
Zerodivide (zero_by_zero) off;

Coefficient (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SXT_IR(m,r,t) # share of margins in exports of good i from region r #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SXT_IR(m,r,t) = VST(m,r,t) / [sum(k,REG, VXWD(m,r,k,t)) + VST(m,r,t)];

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    px_ir(i,r,t) # export price index for good i and region r #;
Equation EXPPRICE_MARG
# price index for total exports of m from r #
(all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    px_ir(m,r,t)
        = sum(s,REG, SX_IRS(m,r,s,t)*pfob(m,r,s,t)) + SXT_IR(m,r,t)*pm(m,r,t);
Equation EXPPRICE_NMRG
# price index for total exports of i from r #
(all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    px_ir(i,r,t) = sum(s,REG, SX_IRS(i,r,s,t) * pfob(i,r,s,t));

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SM_IRS(i,r,s,t) # share of imports of good i into s from r at FOB prices #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,t,ALLTIME)
    SM_IRS(i,r,s,t) = VXWD(i,r,s,t) / VIWDIRALL(i,s,t);

Coefficient (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SMT_IR(m,r,t) # share of transport cost in imports of margin commodity #;
Formula (all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    SMT_IR(m,s,t) = VTICOMM(m,s,t) / VIWDIRALL(m,s,t);

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    pm_ir(i,r,t) # imports price index for good i and region r #;
Equation IMPPRICE1_MARG
# price index for total imports of m in s -- margins commodities #
(all,m,MARG_COMM)(all,s,REG)(all,t,ALLTIME)
    pm_ir(m,s,t)
        = sum(r,REG, SM_IRS(m,r,s,t) * pfob(m,r,s,t)) + SMT_IR(m,s,t) * pt(m,t);
Equation IMPPRICE1_NMRG
# price index for total imports of i in s -- non-margins commodities #
(all,i,NMRG_COMM)(all,s,REG)(all,t,ALLTIME)
    pm_ir(i,s,t) = sum(r,REG, SM_IRS(i,r,s,t) * pfob(i,r,s,t));

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    SW_IR(i,r,t) # share of region r exports in world total for good i #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,t,ALLTIME)
    SW_IR(m,r,t)
        = [sum(s,REG, VXWD(m,r,s,t)) + VST(m,r,t)]
        / sum(k,REG, sum(s,REG, VXWD(m,k,s,t)) + VST(m,k,t));
Formula (all,i,NMRG_COMM)(all,r,REG)(all,t,ALLTIME)
    SW_IR(i,r,t)
        = sum(s,REG, VXWD(i,r,s,t)) / sum(k,REG, sum(s,REG, VXWD(i,k,s,t)));

Variable (all,i,TRAD_COMM)(all,t,ALLTIME)
    px_i(i,t) # world export price index for commodity i #;
Equation WRLDPRICEi
# world export price index for good i #
(all,i,TRAD_COMM)(all,t,ALLTIME)
    px_i(i,t) = sum(r,REG, SW_IR(i,r,t) * px_ir(i,r,t));

Coefficient (all,i,TRAD_COMM)(all,t,ALLTIME)
    SW_I(i,t) # share of exports of i in world total #;
Formula (all,m,MARG_COMM)(all,t,ALLTIME)
    SW_I(m,t)
        = sum(r,REG, sum(s,REG, VXWD(m,r,s,t)) + VST(m,r,t))
        / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s,t))))
            + sum(l,MARG_COMM, sum(r,REG, VST(l,r,t)))];
Formula (all,i,NMRG_COMM)(all,t,ALLTIME)
    SW_I(i,t)
        = sum(r,REG, sum(s,REG, VXWD(i,r,s,t)))
        / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s,t))))
            + sum(l,MARG_COMM, sum(r,REG, VST(l,r,t)))];

Variable (all,t,ALLTIME)
    px_(t) # world export price index for all commodities #;
Equation WRLDPRICE
# world export price index for all goods #
(all,t,ALLTIME)
    px_(t) = sum(i,TRAD_COMM, SW_I(i,t) * px_i(i,t));

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c1_ir(i,r,t) # contribution of world price, px_i, to ToT #;
Equation c1_irEQ
# contribution of world export price index of good i to ToT for region r #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c1_ir(i,r,t) = [SX_IR(i,r,t) - SM_IR(i,r,t)] * [px_i(i,t) - px_(t)];

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c2_ir(i,r,t) # contribution of regional export price, px_ir, to ToT #;
Equation c2_irEQ
# contribution of regional export price of good i for region r #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c2_ir(i,r,t) = SX_IR(i,r,t) * [px_ir(i,r,t) - px_i(i,t)];

Variable (all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c3_ir(i,r,t) # contribution of regional import price, pm_ir, to ToT #;
Equation c3_irEQ
# contribution of imports price index of good i for region r #
(all,i,TRAD_COMM)(all,r,REG)(all,t,ALLTIME)
    c3_ir(i,r,t) = SM_IR(i,r,t) * [pm_ir(i,r,t) - px_i(i,t)];

Variable (all,r,REG)(all,t,ALLTIME)
    c1_r(r,t) # contribution of world prices for all goods to ToT #;
Equation c1_rEQ
# contribution of world price indexes of all goods to ToT for r #
(all,r,REG)(all,t,ALLTIME)
    c1_r(r,t) = sum(i,TRAD_COMM, c1_ir(i,r,t));

Variable (all,r,REG)(all,t,ALLTIME)
    c2_r(r,t) # contribution of regional export prices to ToT #;
Equation c2_rEQ
# contribution of regional exports prices to ToT for r #
(all,r,REG)(all,t,ALLTIME)
    c2_r(r,t) = sum(i,TRAD_COMM, c2_ir(i,r,t));

Variable (all,r,REG)(all,t,ALLTIME)
    c3_r(r,t) # contribution of regional import prices to ToT #;
Equation c3_rEQ
# contribution of regional import prices to ToT for r #
(all,r,REG)(all,t,ALLTIME)
    c3_r(r,t) = sum(i,TRAD_COMM, c3_ir(i,r,t));

Variable (all,r,REG)(all,t,ALLTIME)
    tot2(r,t) # trade terms for region r, computed from components #;
Equation TOT2eq
# trade terms for region r, computed from components #
(all,r,REG)(all,t,ALLTIME)
    tot2(r,t) = c1_r(r,t) + c2_r(r,t) - c3_r(r,t);

!<
Intertemporal module
>!

!<
    -------------------------------
    E. Intertemporal module
    -------------------------------

>!

Variable (all,r,REG)(all,t,ALLTIME)
    mu(r,t) # capital shadow price #;
Variable (all,r,REG)(all,t,ALLTIME)
    capital(r,t) # capital stock #;
Variable (all,r,REG)(all,t,ALLTIME)
    capincr(r,t) # increase in capital stock #;
Variable (all,r,REG)(all,t,FWDTIME)
    delCap(r,t) # dummy variable to calibrate steady state capital value#;
Variable (all,r,REG)(all,t,ALLTIME)
    delMu(r,t) # Dummy variable to calibrate steady state capital value#;


Coefficient (all,r,REG)(all,t,ALLTIME)
    VMU(r,t) # shadow price of capital #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    VCAPINCR(r,t) # value of real capital increment #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    LCAPPRC(r,t) # value of capital price #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    LINVPRC(r,t) # value of investment price #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    MUSHR(r,t) # mu share coefficient #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    PCAPSHR(r,t) # capital price component share coefficient #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    MUSUM(r,t) # sum of RHS mu motion equation #;
Coefficient (all,r,REG)(all,t,FWDTIME)
    CAPISHR(r,t) # qo zCGDS share coefficient #;
Coefficient (all,r,REG)(all,t,FWDTIME)
    LCAPHAT(r,t) # capital shadow price #;
Coefficient (all,r,REG)(all,t,ALLTIME)
    LMUHAT(r,t) # MU slack coefficient #;

!Initial condition switching mechanism: If INIDELTA=1, VKB and the value
of ps("capital",r,t) will be adjusted so the current base year data will become
steady state of the economy. When INIDELTA=0, VKB will be read from data and
the value of ps("capital",r,t) will be calculated by definition. The values of
the auxiliary variables - LCAPHAT(r,t) and LMUHAT(r,t) - should be one. If they
are not 1 then shock them to 1 to get the baseline scenario!

Coefficient (integer,parameter)
    INIDELTA # binary coefficient to switch mechanism of initial condition #;
Formula
    INIDELTA = 0;

Coefficient (parameter)(all,r,REG)(all,t,ALLTIME)
    CPHI(r,t) # investment parameter #;
Formula (all,r,REG)(all,t,ALLTIME)
    CPHI(r,t) = 0.1;

Coefficient (parameter)(all,r,REG)(all,t,ALLTIME)
    KAPPA(r,t) # depreciation rate #;
Formula (all,r,REG)(all,t,ALLTIME)
    KAPPA(r,t) = 0.08;

Coefficient (all,t,ALLTIME)
    LRORG(t) # value of global interest rate #;
Formula (all,t,ALLTIME)
    LRORG(t) = 0.04;

Formula (all,r,REG)(all,t,ALLTIME)
    LINVPRC(r,t) = 1; !Normalised value of ps("zcgds",r,t)!
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    VCAPINCR(r,t) = INIDELTA * VOA("zcgds",r,t) / [1 + CPHI(r,t) * KAPPA(r,t) / 2]
                    + [1 - INIDELTA] * [(VKB(r,t)^2 + 2 * CPHI(r,t) * VKB(r,t) * VOA("zcgds",r,t))^0.5 - VKB(r,t)] / CPHI(r,t);
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    VKB(r,t) = INIDELTA * VCAPINCR(r,t) / KAPPA(r,t) + [1 - INIDELTA] * VKB(r,t);
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    VMU(r,t) = [1 + CPHI(r,t) * VCAPINCR(r,t) / VKB(r,t)] * LINVPRC(r,t);
Formula (initial)(all,r,REG)(all,t,ALLTIME)
    LCAPPRC(r,t) = INIDELTA * [VMU(r,t) * (LRORG(t) + KAPPA(r,t)) - CPHI(r,t) * KAPPA(r,t)^2 / 2 * LINVPRC(r,t)]
                    + [1 - INIDELTA] * EVOA("capital",r,t) / VKB(r,t);
Formula (all,r,REG)(all,t,FWDTIME)
    MUSUM(r,t) = VMU(r,t) * ((LRORG(t) + KAPPA(r,t)) * (YEAR(t+1) - YEAR(t)) + 1)
                 - CPHI(r,t) * [VCAPINCR(r,t) / VKB(r,t)]^2 / 2 * LINVPRC(r,t) * (YEAR(t+1) - YEAR(t))
                 - LCAPPRC(r,t) * (YEAR(t+1) - YEAR(t));
Formula (all,r,REG)(all,t,ENDTIME)
    MUSUM(r,t) = VMU(r,t) * ((LRORG(t) + KAPPA(r,t)) * (YEAR(t) - YEAR(t-1)) + 1)
                 - CPHI(r,t) * [VCAPINCR(r,t) / VKB(r,t)]^2 / 2 * LINVPRC(r,t) * (YEAR(t) - YEAR(t-1))
                 - LCAPPRC(r,t) * (YEAR(t) - YEAR(t-1));
Formula (all,r,REG)(all,t,FWDTIME)
    MUSHR(r,t) = VMU(r,t) * ((LRORG(t) + KAPPA(r,t)) * (YEAR(t+1) - YEAR(t)) + 1) / MUSUM(r,t);
Formula (all,r,REG)(all,t,ENDTIME)
    MUSHR(r,t) = VMU(r,t) * ((LRORG(t) + KAPPA(r,t)) * (YEAR(t) - YEAR(t-1)) + 1) / MUSUM(r,t);
Formula (all,r,REG)(all,t,FWDTIME)
    PCAPSHR(r,t) = -LCAPPRC(r,t) * (YEAR(t+1) - YEAR(t)) / MUSUM(r,t);
Formula (all,r,REG)(all,t,ENDTIME)
    PCAPSHR(r,t) = -LCAPPRC(r,t) * (YEAR(t) - YEAR(t-1)) / MUSUM(r,t);
Formula (all,r,REG)(all,t,FWDTIME)
    CAPISHR(r,t) = [(YEAR(t+1) - YEAR(t)) * VCAPINCR(r,t)]
                   / [(YEAR(t+1) - YEAR(t)) * VCAPINCR(r,t) + (1 - KAPPA(r,t) * (YEAR(t+1) - YEAR(t))) * VKB(r,t)];
Formula (initial)(all,r,REG)(all,t,FWDTIME)
    LCAPHAT(r,t) = VKB(r,t+1)
                   / [(VCAPINCR(r,t) - KAPPA(r,t) * VKB(r,t)) * (YEAR(t+1) - YEAR(t)) + VKB(r,t)];
Formula (initial)(all,r,REG)(all,t,FWDTIME)
    LMUHAT(r,t) = VMU(r,t+1) / MUSUM(r,t);
Formula (initial)(all,r,REG)(all,t,ENDTIME)
    LMUHAT(r,t) = VMU(r,t) / MUSUM(r,t);

Update (all,r,REG)(all,t,ALLTIME)
    VKB(r,t) = qo("capital",r,t) * ps("zcgds",r,t);
Update (all,r,REG)(all,t,ALLTIME)
    VCAPINCR(r,t) = capincr(r,t) * ps("zcgds",r,t);
Update (all,r,REG)(all,t,ALLTIME)
    LCAPPRC(r,t) = ps("capital",r,t);
Update (all,r,REG)(all,t,ALLTIME)
    VMU(r,t) = mu(r,t);
Update (all,t,ALLTIME)
    LRORG(t) = rorg(t);
Update (all,r,REG)(all,t,FWDTIME)
    LCAPHAT(r,t) = delCap(r,t);
Update (all,r,REG)(all,t,ALLTIME)
    LMUHAT(r,t) = delMu(r,t);

Equation E_capstock
# capital stock redefinition #
(all,r,REG)(all,t,ALLTIME)
    qo("capital",r,t) = capital(r,t);

Equation E_inv
# investment function #
(all,r,REG)(all,t,ALLTIME)
    qo("zcgds",r,t) = capincr(r,t) + CPHI(r,t) * VCAPINCR(r,t)
                      / (2 * VKB(r,t) + CPHI(r,t) * VCAPINCR(r,t)) * (capincr(r,t) - capital(r,t));

Equation E_opti
# FOC #
(all,r,REG)(all,t,ALLTIME)
    ps("zcgds",r,t) = mu(r,t) - CPHI(r,t) * VCAPINCR(r,t)
                      / [VKB(r,t) + CPHI(r,t) * VCAPINCR(r,t)] * (capincr(r,t) - capital(r,t));

Equation E_capdot
# capital stock motion equation #
(all,r,REG)(all,t,FWDTIME)
    capital(r,t+1) = CAPISHR(r,t) * capincr(r,t) + [1 - CAPISHR(r,t)] * capital(r,t) + delCap(r,t);

Equation E_mu
# mu motion equation #
(all,r,REG)(all,t,FWDTIME)
    mu(r,t+1) = MUSHR(r,t) * [mu(r,t) + LRORG(t) * (YEAR(t+1) - YEAR(t))
                              / ((LRORG(t) + KAPPA(r,t)) * (YEAR(t+1) - YEAR(t)) + 1) * rorg(t)]
               + [1 - MUSHR(r,t) - PCAPSHR(r,t)] * [ps("zcgds",r,t) + 2 * capincr(r,t) - 2 * capital(r,t)]
               + PCAPSHR(r,t) * ps("capital",r,t) + delmu(r,t);

Equation E_mu_end
# ENDTIME mu #
(all,r,REG)(all,t,ENDTIME)
    mu(r,t) = MUSHR(r,t) * [mu(r,t) + LRORG(t) * (YEAR(t) - YEAR(t-1))
                            / ((LRORG(t) + KAPPA(r,t)) * (YEAR(t) - YEAR(t-1)) + 1) * rorg(t)]
              + [1 - MUSHR(r,t) - PCAPSHR(r,t)] * [ps("zcgds",r,t) + 2 * capincr(r,t) - 2 * capital(r,t)]
              + PCAPSHR(r,t) * ps("capital",r,t) + delmu(r,t);


